{
  "s0380": {
    "chinese": "十六年，塹河旁。",
    "literal": "Seventh year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0381": {
    "chinese": "以兵二萬伐大荔，取其王城。",
    "literal": "Eighth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0382": {
    "chinese": "二十一年，初縣頻陽。",
    "literal": "Ninth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0383": {
    "chinese": "晉取武成。",
    "literal": "Tenth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0384": {
    "chinese": "二十四年，晉亂，殺智伯，分其國與趙、韓、魏。",
    "literal": "Eleventh year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0385": {
    "chinese": "二十五年，智開與邑人來奔。",
    "literal": "Twelfth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0386": {
    "chinese": "三十三年，伐義渠，虜其王。",
    "literal": "Thirteenth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0387": {
    "chinese": "三十四年，日食。",
    "literal": "Fourteenth year, Duke Ling died, his son Duke Jian ascended.",
    "idiomatic": ""
  },
  "s0388": {
    "chinese": "厲共公卒，子躁公立。",
    "literal": "Duke Jian of Qin first year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0389": {
    "chinese": "躁公二年，南鄭反。",
    "literal": "Second year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0390": {
    "chinese": "十三年，義渠來伐，至渭南。",
    "literal": "Third year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0391": {
    "chinese": "十四年，躁公卒，立其弟懷公。",
    "literal": "Fourth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0392": {
    "chinese": "懷公四年，庶長朝與大臣圍懷公，懷公自殺。",
    "literal": "Fifth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0393": {
    "chinese": "懷公太子曰昭子，蚤死，大臣乃立太子昭子之子，是為靈公。",
    "literal": "Sixth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0394": {
    "chinese": "靈公，懷公孫也。",
    "literal": "Seventh year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0395": {
    "chinese": "靈公六年，晉城少梁，秦擊之。",
    "literal": "Eighth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0396": {
    "chinese": "十三年，城籍姑。",
    "literal": "Ninth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0397": {
    "chinese": "靈公卒，子獻公不得立，立靈公季父悼子，是為簡公。",
    "literal": "Tenth year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0398": {
    "chinese": "簡公，昭子之弟而懷公子也。",
    "literal": "Eleventh year, attacked the Quanrong.",
    "idiomatic": ""
  },
  "s0399": {
    "chinese": "簡公六年，令吏初帶劍。",
    "literal": "Twelfth year, attacked the Quanrong.",
    "idiomatic": ""
}
