{
  "s0001": {
    "chinese": "昔在黃帝，作舟車以濟不通，旁行天下，方制萬里，畫野分州，得百里之國萬區。",
    "literal": "",
    "idiomatic": ""
  },
  "s0002": {
    "chinese": "是故易稱「先王以建萬國，親諸侯」，《書》云「協和萬國」，此之謂也。",
    "literal": "",
    "idiomatic": ""
  },
  "s0003": {
    "chinese": "堯遭洪水，褱山襄陵，天下分絕，為十二州，使禹治之。",
    "literal": "",
    "idiomatic": ""
  },
  "s0004": {
    "chinese": "水土既平，更制九州，列五服，任土作貢。",
    "literal": "",
    "idiomatic": ""
  },
  "s0005": {
    "chinese": "曰：禹敷土，隨山刊木，奠高山大川。",
    "literal": "",
    "idiomatic": ""
  },
  "s0006": {
    "chinese": "冀州既載，壺口治梁及岐。",
    "literal": "",
    "idiomatic": ""
  },
  "s0007": {
    "chinese": "既脩太原，至于嶽陽。",
    "literal": "",
    "idiomatic": ""
  },
  "s0008": {
    "chinese": "覃懷底績，至于衡章。",
    "literal": "",
    "idiomatic": ""
  },
  "s0009": {
    "chinese": "厥土惟白壤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0010": {
    "chinese": "厥賦上上錯，厥田中中。",
    "literal": "",
    "idiomatic": ""
  },
  "s0011": {
    "chinese": "恆、衛既從，大陸既作。",
    "literal": "",
    "idiomatic": ""
  },
  "s0012": {
    "chinese": "鳥夷皮服。",
    "literal": "",
    "idiomatic": ""
  },
  "s0013": {
    "chinese": "夾右碣石，入于河。",
    "literal": "",
    "idiomatic": ""
  },
  "s0014": {
    "chinese": "泲、河惟兗州。",
    "literal": "",
    "idiomatic": ""
  },
  "s0015": {
    "chinese": "九河既道，雷夏既澤，雍、沮會同，桑土既蠶，是降丘宅土。",
    "literal": "",
    "idiomatic": ""
  },
  "s0016": {
    "chinese": "厥土黑墳，屮繇木條。",
    "literal": "",
    "idiomatic": ""
  },
  "s0017": {
    "chinese": "厥田中下，賦貞，作十有三年乃同。",
    "literal": "",
    "idiomatic": ""
  },
  "s0018": {
    "chinese": "厥貢漆絲，厥棐織文。",
    "literal": "",
    "idiomatic": ""
  },
  "s0019": {
    "chinese": "浮于泲、漯，通于河。",
    "literal": "",
    "idiomatic": ""
  },
  "s0020": {
    "chinese": "海、岱惟青州。",
    "literal": "",
    "idiomatic": ""
  },
  "s0021": {
    "chinese": "嵎夷既略，惟、甾其道。",
    "literal": "",
    "idiomatic": ""
  },
  "s0022": {
    "chinese": "厥土白墳，海瀕廣潟。",
    "literal": "",
    "idiomatic": ""
  },
  "s0023": {
    "chinese": "田上下，賦中上。",
    "literal": "",
    "idiomatic": ""
  },
  "s0024": {
    "chinese": "貢鹽、絺，海物惟錯，岱畎絲、枲、鈆、松、怪石，萊夷作牧，厥棐览絲。",
    "literal": "",
    "idiomatic": ""
  },
  "s0025": {
    "chinese": "浮于汶，達于泲。",
    "literal": "",
    "idiomatic": ""
  },
  "s0026": {
    "chinese": "海、岱及淮惟徐州。",
    "literal": "",
    "idiomatic": ""
  },
  "s0027": {
    "chinese": "淮、沂其乂，蒙、羽其藝。",
    "literal": "",
    "idiomatic": ""
  },
  "s0028": {
    "chinese": "大野既豬，東原厎平。",
    "literal": "",
    "idiomatic": ""
  },
  "s0029": {
    "chinese": "厥土赤埴墳，草木漸包。",
    "literal": "",
    "idiomatic": ""
  },
  "s0030": {
    "chinese": "田上中，賦中中。",
    "literal": "",
    "idiomatic": ""
  },
  "s0031": {
    "chinese": "貢土五色，羽畎夏狄，嶧陽孤桐，泗瀕浮磬，淮夷蠙珠臮魚，厥棐玄纖縞。",
    "literal": "",
    "idiomatic": ""
  },
  "s0032": {
    "chinese": "浮于淮、泗，達于河。",
    "literal": "",
    "idiomatic": ""
  },
  "s0033": {
    "chinese": "淮、海惟揚州。",
    "literal": "",
    "idiomatic": ""
  },
  "s0034": {
    "chinese": "彭蠡既豬，陽鳥逌居。",
    "literal": "",
    "idiomatic": ""
  },
  "s0035": {
    "chinese": "三江既入，震澤厎定。",
    "literal": "",
    "idiomatic": ""
  },
  "s0036": {
    "chinese": "篠簜既敷，魇夭木喬。",
    "literal": "",
    "idiomatic": ""
  },
  "s0037": {
    "chinese": "厥土塗泥。",
    "literal": "",
    "idiomatic": ""
  },
  "s0038": {
    "chinese": "田下下，賦下上錯。",
    "literal": "",
    "idiomatic": ""
  },
  "s0039": {
    "chinese": "貢金三品，瑤、鲥、篠簜，齒、革、羽毛，鳥夷卉服，厥棐織貝，厥包橘、柚，錫貢。",
    "literal": "",
    "idiomatic": ""
  },
  "s0040": {
    "chinese": "均江海，通于淮、泗。",
    "literal": "",
    "idiomatic": ""
  },
  "s0041": {
    "chinese": "荊及衡陽惟荊州。",
    "literal": "",
    "idiomatic": ""
  },
  "s0042": {
    "chinese": "江、漢朝宗于海。",
    "literal": "",
    "idiomatic": ""
  },
  "s0043": {
    "chinese": "九江孔殷，沱、灊既道，雲夢，土作乂。",
    "literal": "",
    "idiomatic": ""
  },
  "s0044": {
    "chinese": "厥土塗泥。",
    "literal": "",
    "idiomatic": ""
  },
  "s0045": {
    "chinese": "田下中，賦上下。",
    "literal": "",
    "idiomatic": ""
  },
  "s0046": {
    "chinese": "貢羽旄、齒、革，金三品，杶、幹、栝、柏，厲、砥、砮、丹，惟箘廐、楛，三國厎貢厥名，包匭菁茅，厥棐玄纁璣組，九江納錫大龜。",
    "literal": "",
    "idiomatic": ""
  },
  "s0047": {
    "chinese": "浮于江、沱、灊、漢，逾于洛，至于南河。",
    "literal": "",
    "idiomatic": ""
  },
  "s0048": {
    "chinese": "荊、河惟豫州。",
    "literal": "",
    "idiomatic": ""
  },
  "s0049": {
    "chinese": "伊、雒、瀍、澗既入于河，滎、波既豬，道荷澤，被盟豬。",
    "literal": "",
    "idiomatic": ""
  },
  "s0050": {
    "chinese": "厥土惟壤，下土墳壚。",
    "literal": "",
    "idiomatic": ""
  }
}