  },
  "s0770": {
    "chinese": "大梁，初胃七度，穀雨。",
    "literal": "",
    "idiomatic": ""
  },
  "s0771": {
    "chinese": "中昴八度，清明。",
    "literal": "",
    "idiomatic": ""
  },
  "s0772": {
    "chinese": "終於畢十一度。",
    "literal": "",
    "idiomatic": ""
  },
  "s0773": {
    "chinese": "實沈、初畢十二度，立夏。",
    "literal": "",
    "idiomatic": ""
  },
  "s0774": {
    "chinese": "中井初，小滿。",
    "literal": "",
    "idiomatic": ""
  },
  "s0775": {
    "chinese": "終於井十五度。",
    "literal": "",
    "idiomatic": ""
  },
  "s0776": {
    "chinese": "鶉首，初井十六度，芒種。",
    "literal": "",
    "idiomatic": ""
  },
  "s0777": {
