{
  "s0251": {
    "chinese": "承帝明德，師象山則。",
    "literal": "",
    "idiomatic": ""
  },
  "s0252": {
    "chinese": "雲施稱民，永受厥福。",
    "literal": "",
    "idiomatic": ""
  },
  "s0253": {
    "chinese": "承容之常，承帝之明。",
    "literal": "",
    "idiomatic": ""
  },
  "s0254": {
    "chinese": "下民安樂，受福無疆。",
    "literal": "",
    "idiomatic": ""
  },
  "s0255": {
    "chinese": "郊祀歌十九章，其詩曰：",
    "literal": "",
    "idiomatic": ""
  },
  "s0256": {
    "chinese": "練時日，侯有望，巽膋蕭，延四方。",
    "literal": "",
    "idiomatic": ""
  },
  "s0257": {
    "chinese": "九重開，靈之斿，垂惠恩，鴻祜休。",
    "literal": "",
    "idiomatic": ""
  },
  "s0258": {
    "chinese": "靈之車，結玄雲，駕飛龍，羽旄紛。",
    "literal": "",
    "idiomatic": ""
  },
  "s0259": {
    "chinese": "靈之下，若風馬，左倉龍，右白虎。",
    "literal": "",
    "idiomatic": ""
  },
  "s0260": {
    "chinese": "靈之來，神哉沛，先以雨，般裔裔。",
    "literal": "",
    "idiomatic": ""
  },
  "s0261": {
    "chinese": "靈之至，慶陰陰，相放怫，震澹心。",
    "literal": "",
    "idiomatic": ""
  },
  "s0262": {
    "chinese": "靈已坐，五音飭，虞至旦，承靈億。",
    "literal": "",
    "idiomatic": ""
  },
  "s0263": {
    "chinese": "牲繭栗，粢盛香，尊桂酒，賓八鄉。",
    "literal": "",
    "idiomatic": ""
  },
  "s0264": {
    "chinese": "靈安留，吟青黃，遍觀此，眺瑤堂。",
    "literal": "",
    "idiomatic": ""
  },
  "s0265": {
    "chinese": "眾嫭並，綽奇麗，顏如荼，兆逐靡。",
    "literal": "",
    "idiomatic": ""
  },
  "s0266": {
    "chinese": "被華文，廁霧縠，曳阿錫，佩珠玉。",
    "literal": "",
    "idiomatic": ""
  },
  "s0267": {
    "chinese": "俠嘉夜，仂蘭芳，澹容與，獻嘉觴。",
    "literal": "",
    "idiomatic": ""
  },
  "s0268": {
    "chinese": "練時日一",
    "literal": "",
    "idiomatic": ""
  },
  "s0269": {
    "chinese": "帝臨中壇，四方承宇，繩繩意變，備得其所。",
    "literal": "",
    "idiomatic": ""
  },
  "s0270": {
    "chinese": "清和六合，制數以五。",
    "literal": "",
    "idiomatic": ""
  },
  "s0271": {
    "chinese": "海內安寧，興文匽武。",
    "literal": "",
    "idiomatic": ""
  },
  "s0272": {
    "chinese": "后土富媼，昭明三光。",
    "literal": "",
    "idiomatic": ""
  },
  "s0273": {
    "chinese": "穆穆優游，嘉服上黃。",
    "literal": "",
    "idiomatic": ""
  },
  "s0274": {
    "chinese": "帝臨二",
    "literal": "",
    "idiomatic": ""
  },
  "s0275": {
    "chinese": "青陽開動，根荄以遂，膏潤并愛，跂行畢逮。",
    "literal": "",
    "idiomatic": ""
  },
  "s0276": {
    "chinese": "霆聲發榮，壧處頃聽，枯槁復產，乃成厥命。",
    "literal": "",
    "idiomatic": ""
  },
  "s0277": {
    "chinese": "眾庶熙熙，施及夭胎，群生啿啿，惟春之祺。",
    "literal": "",
    "idiomatic": ""
  },
  "s0278": {
    "chinese": "青陽三鄒子樂。",
    "literal": "",
    "idiomatic": ""
  },
  "s0279": {
    "chinese": "朱明盛長，归與萬物，桐生茂豫，靡有所詘。",
    "literal": "",
    "idiomatic": ""
  },
  "s0280": {
    "chinese": "敷華就實，既阜既昌，登成甫田，百鬼迪嘗。",
    "literal": "",
    "idiomatic": ""
  },
  "s0281": {
    "chinese": "廣大建祀，肅雍不忘，神若宥之，傳世無疆。",
    "literal": "",
    "idiomatic": ""
  },
  "s0282": {
    "chinese": "朱明四鄒子樂。",
    "literal": "",
    "idiomatic": ""
  },
  "s0283": {
    "chinese": "西顥沆碭，秋氣肅殺，含秀垂穎，續舊不廢。",
    "literal": "",
    "idiomatic": ""
  },
  "s0284": {
    "chinese": "姦偽不萌，祅孽伏息，隅辟越遠，四貉咸服。",
    "literal": "",
    "idiomatic": ""
  },
  "s0285": {
    "chinese": "既畏茲威，惟慕純德，附而不驕，正心翊翊。",
    "literal": "",
    "idiomatic": ""
  },
  "s0286": {
    "chinese": "西顥五鄒子樂。",
    "literal": "",
    "idiomatic": ""
  },
  "s0287": {
    "chinese": "玄冥陵陰，蟄蟲蓋臧，屮木零落，抵冬降霜。",
    "literal": "",
    "idiomatic": ""
  },
  "s0288": {
    "chinese": "易亂除邪，革正異俗，兆民反本，抱素懷樸。",
    "literal": "",
    "idiomatic": ""
  },
  "s0289": {
    "chinese": "條理信義，望禮五嶽。",
    "literal": "",
    "idiomatic": ""
  },
  "s0290": {
    "chinese": "籍斂之時，掩收嘉穀。",
    "literal": "",
    "idiomatic": ""
  },
  "s0291": {
    "chinese": "玄冥六鄒子樂。",
    "literal": "",
    "idiomatic": ""
  },
  "s0292": {
    "chinese": "惟泰元尊，媼神蕃釐，經緯天地，作成四時。",
    "literal": "",
    "idiomatic": ""
  },
  "s0293": {
    "chinese": "精建日月，星辰度理，陰陽五行，周而復始。",
    "literal": "",
    "idiomatic": ""
  },
  "s0294": {
    "chinese": "雲風雷電，降甘露雨，百姓蕃滋，咸循厥緒。",
    "literal": "",
    "idiomatic": ""
  },
  "s0295": {
    "chinese": "繼統共勤，順皇之德，鸞路龍鱗，罔不肸飾。",
    "literal": "",
    "idiomatic": ""
  },
  "s0296": {
    "chinese": "嘉籩列陳，庶幾宴享，滅除凶災，列騰八荒。",
    "literal": "",
    "idiomatic": ""
  },
  "s0297": {
    "chinese": "鐘鼓竽笙，雲舞翔翔，招搖靈旗，九夷賓將。",
    "literal": "",
    "idiomatic": ""
  },
  "s0298": {
    "chinese": "惟泰元七建始元年，丞相匡衡奏罷「鸞路龍鱗」，更定詩曰「涓選休成」。",
    "literal": "",
    "idiomatic": ""
  },
  "s0299": {
    "chinese": "天地並況，惟予有慕，爰熙紫壇，思求厥路。",
    "literal": "",
    "idiomatic": ""
  },
  "s0300": {
    "chinese": "恭承禋祀，縕豫為紛，黼繡周張，承神至尊。",
    "literal": "",
    "idiomatic": ""
  }
}