{
  "s0301": {
    "chinese": "千童羅舞成八溢，合好效歡虞泰一。",
    "literal": "",
    "idiomatic": ""
  },
  "s0302": {
    "chinese": "九歌畢奏斐然殊，鳴琴竽瑟會軒朱。",
    "literal": "",
    "idiomatic": ""
  },
  "s0303": {
    "chinese": "璆磬金鼓，靈其有喜，百官濟濟，各敬厥事。",
    "literal": "",
    "idiomatic": ""
  },
  "s0304": {
    "chinese": "盛牲實俎進聞膏，神奄留，臨須搖。",
    "literal": "",
    "idiomatic": ""
  },
  "s0305": {
    "chinese": "長麗前掞光燿明，寒暑不忒況皇章。",
    "literal": "",
    "idiomatic": ""
  },
  "s0306": {
    "chinese": "展詩應律鋗玉鳴，函宮吐角激徵清。",
    "literal": "",
    "idiomatic": ""
  },
  "s0307": {
    "chinese": "發梁揚羽申以商，造茲新音永久長。",
    "literal": "",
    "idiomatic": ""
  },
  "s0308": {
    "chinese": "聲氣遠條鳳鳥启，神夕奄虞蓋孔享。",
    "literal": "",
    "idiomatic": ""
  },
  "s0309": {
    "chinese": "天地八丞相匡衡奏罷「黼繡周張」，更定詩曰「肅若舊典」。",
    "literal": "",
    "idiomatic": ""
  },
  "s0310": {
    "chinese": "日出入安窮？",
    "literal": "",
    "idiomatic": ""
  },
  "s0311": {
    "chinese": "時世不與人同。",
    "literal": "",
    "idiomatic": ""
  },
  "s0312": {
    "chinese": "故春非我春，夏非我夏，秋非我秋，冬非我冬。",
    "literal": "",
    "idiomatic": ""
  },
  "s0313": {
    "chinese": "泊如四海之池，遍觀是邪謂何？",
    "literal": "",
    "idiomatic": ""
  },
  "s0314": {
    "chinese": "吾知所樂，獨樂六龍，六龍之調，使我心若。",
    "literal": "",
    "idiomatic": ""
  },
  "s0315": {
    "chinese": "訾黃其何不徠下！",
    "literal": "",
    "idiomatic": ""
  },
  "s0316": {
    "chinese": "日出入九",
    "literal": "",
    "idiomatic": ""
  },
  "s0317": {
    "chinese": "太一況，天馬下，霑赤汗，沬流赭。",
    "literal": "",
    "idiomatic": ""
  },
  "s0318": {
    "chinese": "志俶儻，精權奇，逦浮雲，晻上馳。",
    "literal": "",
    "idiomatic": ""
  },
  "s0319": {
    "chinese": "體容與，迣萬里，今安匹，龍為友。",
    "literal": "",
    "idiomatic": ""
  },
  "s0320": {
    "chinese": "元狩三年馬生渥洼水中作。",
    "literal": "",
    "idiomatic": ""
  },
  "s0321": {
    "chinese": "天馬徠，從西極，涉流沙，九夷服。",
    "literal": "",
    "idiomatic": ""
  },
  "s0322": {
    "chinese": "天馬徠，出泉水，虎脊兩，化若鬼。",
    "literal": "",
    "idiomatic": ""
  },
  "s0323": {
    "chinese": "天馬徠，歷無草，徑千里，循東道。",
    "literal": "",
    "idiomatic": ""
  },
  "s0324": {
    "chinese": "天馬徠，執徐時，將搖舉，誰與期？",
    "literal": "",
    "idiomatic": ""
  },
  "s0325": {
    "chinese": "天馬徠，開遠門，竦予身，逝昆侖。",
    "literal": "",
    "idiomatic": ""
  },
  "s0326": {
    "chinese": "天馬徠，龍之媒，游閶闔，觀玉臺。",
    "literal": "",
    "idiomatic": ""
  },
  "s0327": {
    "chinese": "太初四年誅宛王獲宛馬作。",
    "literal": "",
    "idiomatic": ""
  },
  "s0328": {
    "chinese": "天馬十",
    "literal": "",
    "idiomatic": ""
  },
  "s0329": {
    "chinese": "天門開，詄蕩蕩，穆並騁，以臨饗。",
    "literal": "",
    "idiomatic": ""
  },
  "s0330": {
    "chinese": "光夜燭，德信著，靈娅平而鴻，長生豫。",
    "literal": "",
    "idiomatic": ""
  },
  "s0331": {
    "chinese": "大朱涂廣，夷石為堂，飾玉梢以舞歌，體招搖若永望。",
    "literal": "",
    "idiomatic": ""
  },
  "s0332": {
    "chinese": "星留俞，塞隕光，照紫幄，珠熉黃。",
    "literal": "",
    "idiomatic": ""
  },
  "s0333": {
    "chinese": "幡比还集，貳雙飛常羊。",
    "literal": "",
    "idiomatic": ""
  },
  "s0334": {
    "chinese": "月穆穆以金波，日華燿以宣明。",
    "literal": "",
    "idiomatic": ""
  },
  "s0335": {
    "chinese": "假清風軋忽，激長至重觴。",
    "literal": "",
    "idiomatic": ""
  },
  "s0336": {
    "chinese": "神裴回若留放，殣冀親以肆章。",
    "literal": "",
    "idiomatic": ""
  },
  "s0337": {
    "chinese": "函蒙祉福常若期，寂漻上天知厥時。",
    "literal": "",
    "idiomatic": ""
  },
  "s0338": {
    "chinese": "泛泛滇滇從高斿，殷勤此路臚所求。",
    "literal": "",
    "idiomatic": ""
  },
  "s0339": {
    "chinese": "佻正嘉吉弘以昌，休嘉砰隱溢四方。",
    "literal": "",
    "idiomatic": ""
  },
  "s0340": {
    "chinese": "專精厲意逝九閡，紛云六幕浮大海。",
    "literal": "",
    "idiomatic": ""
  },
  "s0341": {
    "chinese": "天門十一",
    "literal": "",
    "idiomatic": ""
  },
  "s0342": {
    "chinese": "景星顯見，信星彪列，象載昭庭，日親以察。",
    "literal": "",
    "idiomatic": ""
  },
  "s0343": {
    "chinese": "參侔開闔，爰推本紀，汾脽出鼎，皇祜元始。",
    "literal": "",
    "idiomatic": ""
  },
  "s0344": {
    "chinese": "五音六律，依韋饗昭，雜變並會，雅聲遠姚。",
    "literal": "",
    "idiomatic": ""
  },
  "s0345": {
    "chinese": "空桑琴瑟結信成，四興遞代八風生。",
    "literal": "",
    "idiomatic": ""
  },
  "s0346": {
    "chinese": "殷殷鐘石羽籥鳴。",
    "literal": "",
    "idiomatic": ""
  },
  "s0347": {
    "chinese": "河龍供鯉醇犧牲。",
    "literal": "",
    "idiomatic": ""
  },
  "s0348": {
    "chinese": "百末旨酒布蘭生。",
    "literal": "",
    "idiomatic": ""
  },
  "s0349": {
    "chinese": "泰尊柘漿析朝酲。",
    "literal": "",
    "idiomatic": ""
  },
  "s0350": {
    "chinese": "微感心攸通修名，周流常羊思所并。",
    "literal": "",
    "idiomatic": ""
  }
}