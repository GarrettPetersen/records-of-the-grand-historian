  },
  "sentences": [
    {
      "id": "10-s0061",
      "originalId": "s0061",
      "blockIndex": 10,
      "chinese": "中，端門；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0062",
      "originalId": "s0062",
      "blockIndex": 10,
      "chinese": "門左右，掖門。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0063",
      "originalId": "s0063",
      "blockIndex": 10,
      "chinese": "門內六星，諸侯。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0064",
      "originalId": "s0064",
      "blockIndex": 10,
      "chinese": "其內五星，五帝坐。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0065",
      "originalId": "s0065",
      "blockIndex": 10,
      "chinese": "後聚一十五星，蔚然，曰郎位；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0066",
      "originalId": "s0066",
      "blockIndex": 10,
      "chinese": "傍一大星，將位也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0067",
      "originalId": "s0067",
      "blockIndex": 10,
      "chinese": "月、五星順入，軌道，司其出，所守，天子所誅也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0068",
      "originalId": "s0068",
      "blockIndex": 10,
      "chinese": "其逆入，若不軌道，以所犯命之；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0069",
      "originalId": "s0069",
      "blockIndex": 10,
      "chinese": "中坐，成形，皆群下從謀也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0070",
      "originalId": "s0070",
      "blockIndex": 10,
      "chinese": "金、火尤甚。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0071",
      "originalId": "s0071",
      "blockIndex": 10,
      "chinese": "廷藩西有隋星五，曰少微，士大夫。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0072",
      "originalId": "s0072",
      "blockIndex": 10,
      "chinese": "權，軒轅。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0073",
      "originalId": "s0073",
      "blockIndex": 10,
      "chinese": "軒轅，黃龍體。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0074",
      "originalId": "s0074",
      "blockIndex": 10,
      "chinese": "前大星，女主象；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0075",
      "originalId": "s0075",
      "blockIndex": 10,
      "chinese": "旁小星，御者後宮屬。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}