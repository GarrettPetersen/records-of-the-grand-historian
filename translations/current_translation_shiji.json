  "sentences": [
    {
      "id": "20-s0136",
      "originalId": "s0136",
      "blockIndex": 20,
      "chinese": "其南有眾星，曰羽林天軍。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "20-s0137",
      "originalId": "s0137",
      "blockIndex": 20,
      "chinese": "軍西為壘，或曰鉞。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "20-s0138",
      "originalId": "s0138",
      "blockIndex": 20,
      "chinese": "旁有一大星為北落。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "20-s0139",
      "originalId": "s0139",
      "blockIndex": 20,
      "chinese": "北落若微亡，軍星動角益希，及五星犯北落，入軍，軍起。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "20-s0140",
      "originalId": "s0140",
      "blockIndex": 20,
      "chinese": "火、金、水尤甚：火，軍憂；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "20-s0141",
      "originalId": "s0141",
      "blockIndex": 20,
      "chinese": "水，［水］患；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "20-s0142",
      "originalId": "s0142",
      "blockIndex": 20,
      "chinese": "木、土，軍吉。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "20-s0143",
      "originalId": "s0143",
      "blockIndex": 20,
      "chinese": "危東六星，兩兩相比，曰司空。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "21-s0144",
      "originalId": "s0144",
      "blockIndex": 21,
      "chinese": "營室為清廟，曰離宮、閣道。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "21-s0145",
      "originalId": "s0145",
      "blockIndex": 21,
      "chinese": "漢中四星，曰天駟。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "21-s0146",
      "originalId": "s0146",
      "blockIndex": 21,
      "chinese": "旁一星，曰王良。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "21-s0147",
      "originalId": "s0147",
      "blockIndex": 21,
      "chinese": "王良策馬，車騎滿野。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "21-s0148",
      "originalId": "s0148",
      "blockIndex": 21,
      "chinese": "旁有八星，絕漢，曰天潢。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "21-s0149",
      "originalId": "s0149",
      "blockIndex": 21,
      "chinese": "天潢旁，江星。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "21-s0150",
      "originalId": "s0150",
      "blockIndex": 21,
      "chinese": "江星動，人涉水。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}