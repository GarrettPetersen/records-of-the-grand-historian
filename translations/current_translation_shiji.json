  "sentences": [
    {
      "id": "27-s0181",
      "originalId": "s0181",
      "blockIndex": 27,
      "chinese": "單閼歲：歲陰在卯，星居子。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "27-s0182",
      "originalId": "s0182",
      "blockIndex": 27,
      "chinese": "以二月與婺女、虛、危晨出，曰降入。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "27-s0183",
      "originalId": "s0183",
      "blockIndex": 27,
      "chinese": "大有光。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "27-s0184",
      "originalId": "s0184",
      "blockIndex": 27,
      "chinese": "其失次，有應見張。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "27-s0185",
      "originalId": "s0185",
      "blockIndex": 27,
      "chinese": "［名曰降入］其歲大水。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "28-s0186",
      "originalId": "s0186",
      "blockIndex": 28,
      "chinese": "執徐歲：歲陰在辰，星居亥。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "28-s0187",
      "originalId": "s0187",
      "blockIndex": 28,
      "chinese": "以三月［居］與營室、東壁晨出，曰青章。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "28-s0188",
      "originalId": "s0188",
      "blockIndex": 28,
      "chinese": "青青甚章。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "28-s0189",
      "originalId": "s0189",
      "blockIndex": 28,
      "chinese": "其失次；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "28-s0190",
      "originalId": "s0190",
      "blockIndex": 28,
      "chinese": "有應見軫。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "28-s0191",
      "originalId": "s0191",
      "blockIndex": 28,
      "chinese": "［曰青章］歲早，旱；",
