  "sentences": [
    {
      "id": "7-s0046",
      "originalId": "s0046",
      "blockIndex": 7,
      "chinese": "其兩旁各有三星，鼎足句之，曰攝提。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "7-s0047",
      "originalId": "s0047",
      "blockIndex": 7,
      "chinese": "攝提者，直斗杓所指，以建時節，故曰「攝提格」。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "7-s0048",
      "originalId": "s0048",
      "blockIndex": 7,
      "chinese": "亢為疏廟，主疾。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "7-s0049",
      "originalId": "s0049",
      "blockIndex": 7,
      "chinese": "其南北兩大星，曰南門。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "7-s0050",
      "originalId": "s0050",
      "blockIndex": 7,
      "chinese": "氐為天根，主疫。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "8-s0051",
      "originalId": "s0051",
      "blockIndex": 8,
      "chinese": "尾為九子，曰君臣；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "8-s0052",
      "originalId": "s0052",
      "blockIndex": 8,
      "chinese": "斥絕，不和。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "8-s0053",
      "originalId": "s0053",
      "blockIndex": 8,
      "chinese": "箕為敖客，曰口舌。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "9-s0054",
      "originalId": "s0054",
      "blockIndex": 9,
      "chinese": "火犯守角，則有戰。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "9-s0055",
      "originalId": "s0055",
      "blockIndex": 9,
      "chinese": "房、心，王者惡之也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0056",
      "originalId": "s0056",
      "blockIndex": 10,
      "chinese": "南宮朱鳥，權、衡。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0057",
      "originalId": "s0057",
      "blockIndex": 10,
      "chinese": "衡，太微，三光之廷。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0058",
      "originalId": "s0058",
      "blockIndex": 10,
      "chinese": "匡衛十二星，藩臣：西，將；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0059",
      "originalId": "s0059",
      "blockIndex": 10,
      "chinese": "東，相；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "10-s0060",
      "originalId": "s0060",
      "blockIndex": 10,
      "chinese": "南四星，執法；",
      "literal": "",
      "idiomatic": ""
    }
  ]
}