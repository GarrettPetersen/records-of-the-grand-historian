  "sentences": [
    {
      "id": "13-s0091",
      "originalId": "s0091",
      "blockIndex": 13,
      "chinese": "軫為車，主風。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "13-s0092",
      "originalId": "s0092",
      "blockIndex": 13,
      "chinese": "其旁有一小星，曰長沙，星星不欲明；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "13-s0093",
      "originalId": "s0093",
      "blockIndex": 13,
      "chinese": "明與四星等，若五星入軫中，兵大起。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "13-s0094",
      "originalId": "s0094",
      "blockIndex": 13,
      "chinese": "軫南眾星曰天庫樓；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "13-s0095",
      "originalId": "s0095",
      "blockIndex": 13,
      "chinese": "庫有五車。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "13-s0096",
      "originalId": "s0096",
      "blockIndex": 13,
      "chinese": "車星角若益眾，及不具，無處車馬。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "14-s0097",
      "originalId": "s0097",
      "blockIndex": 14,
      "chinese": "西宮咸池，曰天五潢。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "14-s0098",
      "originalId": "s0098",
      "blockIndex": 14,
      "chinese": "五潢，五帝車舍。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "14-s0099",
      "originalId": "s0099",
      "blockIndex": 14,
      "chinese": "火入，旱；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "14-s0100",
      "originalId": "s0100",
      "blockIndex": 14,
      "chinese": "金，兵；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "14-s0101",
      "originalId": "s0101",
      "blockIndex": 14,
      "chinese": "水，水。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "14-s0102",
      "originalId": "s0102",
      "blockIndex": 14,
      "chinese": "中有三柱；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "14-s0103",
      "originalId": "s0103",
      "blockIndex": 14,
      "chinese": "柱不具，兵起。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "15-s0104",
      "originalId": "s0104",
      "blockIndex": 15,
      "chinese": "奎曰封豕，為溝瀆。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "15-s0105",
      "originalId": "s0105",
      "blockIndex": 15,
      "chinese": "婁為聚眾。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}