  "sentences": [
    {
      "id": "10-s0076",
      "originalId": "s0076",
      "blockIndex": 10,
      "chinese": "月、五星守犯者，如衡占。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0077",
      "originalId": "s0077",
      "blockIndex": 11,
      "chinese": "東井為水事。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0078",
      "originalId": "s0078",
      "blockIndex": 11,
      "chinese": "其西曲星曰鉞。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0079",
      "originalId": "s0079",
      "blockIndex": 11,
      "chinese": "鉞北，北河；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0080",
      "originalId": "s0080",
      "blockIndex": 11,
      "chinese": "南，南河；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0081",
      "originalId": "s0081",
      "blockIndex": 11,
      "chinese": "兩河、天闕閒為關梁。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0082",
      "originalId": "s0082",
      "blockIndex": 11,
      "chinese": "輿鬼，鬼祠事；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0083",
      "originalId": "s0083",
      "blockIndex": 11,
      "chinese": "中白者為質。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0084",
      "originalId": "s0084",
      "blockIndex": 11,
      "chinese": "火守南北河，兵起，穀不登。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "11-s0085",
      "originalId": "s0085",
      "blockIndex": 11,
      "chinese": "故德成衡，觀成潢，傷成鉞，禍成井，誅成質。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "12-s0086",
      "originalId": "s0086",
      "blockIndex": 12,
      "chinese": "柳為鳥注，主木草。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "12-s0087",
      "originalId": "s0087",
      "blockIndex": 12,
      "chinese": "七星，頸，為員官。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "12-s0088",
      "originalId": "s0088",
      "blockIndex": 12,
      "chinese": "主急事。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "12-s0089",
      "originalId": "s0089",
      "blockIndex": 12,
      "chinese": "張，素，為廚，主觴客。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "12-s0090",
      "originalId": "s0090",
      "blockIndex": 12,
      "chinese": "翼為羽翮，主遠客。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}