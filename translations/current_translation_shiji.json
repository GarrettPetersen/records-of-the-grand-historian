{
  "metadata": {
    "book": "shiji",
    "chapter": "027",
    "file": "data/shiji/027.json"
  },
  "sentences": [
    {
      "id": "22-s0151",
      "originalId": "s0151",
      "blockIndex": 22,
      "chinese": "杵、臼四星，在危南。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "22-s0152",
      "originalId": "s0152",
      "blockIndex": 22,
      "chinese": "匏瓜，有青黑星守之，魚鹽貴。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0153",
      "originalId": "s0153",
      "blockIndex": 23,
      "chinese": "南斗為廟，其北建星。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0154",
      "originalId": "s0154",
      "blockIndex": 23,
      "chinese": "建星者，旗也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0155",
      "originalId": "s0155",
      "blockIndex": 23,
      "chinese": "牽牛為犧牲。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0156",
      "originalId": "s0156",
      "blockIndex": 23,
      "chinese": "其北河鼓。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0157",
      "originalId": "s0157",
      "blockIndex": 23,
      "chinese": "河鼓大星，上將；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0158",
      "originalId": "s0158",
      "blockIndex": 23,
      "chinese": "左右，左右將。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0159",
      "originalId": "s0159",
      "blockIndex": 23,
      "chinese": "婺女，其北織女。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "23-s0160",
      "originalId": "s0160",
      "blockIndex": 23,
      "chinese": "織女，天女孫也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "24-s0161",
      "originalId": "s0161",
      "blockIndex": 24,
      "chinese": "察日、月之行以揆歲星順逆。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "24-s0162",
      "originalId": "s0162",
      "blockIndex": 24,
      "chinese": "曰東方木，主春，日甲乙。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "24-s0163",
      "originalId": "s0163",
      "blockIndex": 24,
      "chinese": "義失者，罰出歲星。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "24-s0164",
      "originalId": "s0164",
      "blockIndex": 24,
      "chinese": "歲星贏縮，以其捨命國。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "24-s0165",
      "originalId": "s0165",
      "blockIndex": 24,
      "chinese": "所在國不可伐，可以罰人。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}