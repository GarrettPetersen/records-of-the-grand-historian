{
  "metadata": {
    "book": "shiji",
    "chapter": "027",
    "file": "data/shiji/027.json"
  },
  "sentences": [
    {
      "id": "15-s0106",
      "originalId": "s0106",
      "blockIndex": 15,
      "chinese": "胃為天倉。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "15-s0107",
      "originalId": "s0107",
      "blockIndex": 15,
      "chinese": "其南眾星曰廥積。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "16-s0108",
      "originalId": "s0108",
      "blockIndex": 16,
      "chinese": "昴曰髦頭，胡星也，為白衣會。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "16-s0109",
      "originalId": "s0109",
      "blockIndex": 16,
      "chinese": "畢曰罕車，為邊兵，主弋獵。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "16-s0110",
      "originalId": "s0110",
      "blockIndex": 16,
      "chinese": "其大星旁小星為附耳。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "16-s0111",
      "originalId": "s0111",
      "blockIndex": 16,
      "chinese": "附耳搖動，有讒亂臣在側。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "16-s0112",
      "originalId": "s0112",
      "blockIndex": 16,
      "chinese": "昴、畢閒為天街。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "16-s0113",
      "originalId": "s0113",
      "blockIndex": 16,
      "chinese": "其陰，陰國；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "16-s0114",
      "originalId": "s0114",
      "blockIndex": 16,
      "chinese": "陽，陽國。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0115",
      "originalId": "s0115",
      "blockIndex": 17,
      "chinese": "參為白虎。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0116",
      "originalId": "s0116",
      "blockIndex": 17,
      "chinese": "三星直者，是為衡石。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0117",
      "originalId": "s0117",
      "blockIndex": 17,
      "chinese": "下有三星，兌，曰罰，為斬艾事。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0118",
      "originalId": "s0118",
      "blockIndex": 17,
      "chinese": "其外四星，左右肩股也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0119",
      "originalId": "s0119",
      "blockIndex": 17,
      "chinese": "小三星隅置，曰觜觿，為虎首，主葆旅事。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0120",
      "originalId": "s0120",
      "blockIndex": 17,
      "chinese": "其南有四星，曰天廁。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}