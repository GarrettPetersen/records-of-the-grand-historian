  "sentences": [
    {
      "id": "35-s0226",
      "originalId": "s0226",
      "blockIndex": 35,
      "chinese": "以十月與角、亢晨出，曰大章。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "35-s0227",
      "originalId": "s0227",
      "blockIndex": 35,
      "chinese": "蒼蒼然，星若躍而陰出旦，是謂「正平」。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "35-s0228",
      "originalId": "s0228",
      "blockIndex": 35,
      "chinese": "起師旅，其率必武；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "35-s0229",
      "originalId": "s0229",
      "blockIndex": 35,
      "chinese": "其國有德，將有四海。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "35-s0230",
      "originalId": "s0230",
      "blockIndex": 35,
      "chinese": "其失次，有應見婁。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "36-s0231",
      "originalId": "s0231",
      "blockIndex": 36,
      "chinese": "困敦歲：歲陰在子，星居卯。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "36-s0232",
      "originalId": "s0232",
      "blockIndex": 36,
      "chinese": "以十一月與氐、房、心晨出，曰天泉。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "36-s0233",
      "originalId": "s0233",
      "blockIndex": 36,
      "chinese": "玄色甚明。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "36-s0234",
      "originalId": "s0234",
      "blockIndex": 36,
      "chinese": "江池其昌，不利起兵。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "36-s0235",
      "originalId": "s0235",
      "blockIndex": 36,
      "chinese": "其失次，有應（在）［見］昴。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "37-s0236",
      "originalId": "s0236",
      "blockIndex": 37,
      "chinese": "赤奮若歲：歲陰在丑，星居寅，以十二月與尾、箕晨出，曰天皓。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "37-s0237",
      "originalId": "s0237",
      "blockIndex": 37,
      "chinese": "黫然黑色甚明。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "37-s0238",
      "originalId": "s0238",
      "blockIndex": 37,
      "chinese": "其失次，有應見參。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "38-s0239",
      "originalId": "s0239",
      "blockIndex": 38,
      "chinese": "當居不居，居之又左右搖，未當去去之，與他星會，其國凶。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "38-s0240",
      "originalId": "s0240",
      "blockIndex": 38,
      "chinese": "所居久，國有德厚。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}