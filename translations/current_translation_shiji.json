  "sentences": [
    {
      "id": "6-s0031",
      "originalId": "s0031",
      "blockIndex": 6,
      "chinese": "心為明堂，大星天王，前後星子屬。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0032",
      "originalId": "s0032",
      "blockIndex": 6,
      "chinese": "不欲直，直則天王失計。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0033",
      "originalId": "s0033",
      "blockIndex": 6,
      "chinese": "房為府，曰天駟。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0034",
      "originalId": "s0034",
      "blockIndex": 6,
      "chinese": "其陰，右驂。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0035",
      "originalId": "s0035",
      "blockIndex": 6,
      "chinese": "旁有兩星曰衿；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0036",
      "originalId": "s0036",
      "blockIndex": 6,
      "chinese": "北一星曰舝。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0037",
      "originalId": "s0037",
      "blockIndex": 6,
      "chinese": "東北曲十二星曰旗。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0038",
      "originalId": "s0038",
      "blockIndex": 6,
      "chinese": "旗中四星天市；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0039",
      "originalId": "s0039",
      "blockIndex": 6,
      "chinese": "中六星曰市樓。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0040",
      "originalId": "s0040",
      "blockIndex": 6,
      "chinese": "市中星眾者實；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0041",
      "originalId": "s0041",
      "blockIndex": 6,
      "chinese": "其虛則秏。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "6-s0042",
      "originalId": "s0042",
      "blockIndex": 6,
      "chinese": "房南眾星曰騎官。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "7-s0043",
      "originalId": "s0043",
      "blockIndex": 7,
      "chinese": "左角，李；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "7-s0044",
      "originalId": "s0044",
      "blockIndex": 7,
      "chinese": "右角，將。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "7-s0045",
      "originalId": "s0045",
      "blockIndex": 7,
      "chinese": "大角者，天王帝廷。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}