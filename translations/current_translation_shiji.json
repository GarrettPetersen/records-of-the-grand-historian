{
  "metadata": {
    "book": "shiji",
    "chapter": "027",
    "file": "data/shiji/027.json"
  },
  "sentences": [
    {
      "id": "48-s0316",
      "originalId": "s0316",
      "blockIndex": 48,
      "chinese": "入，得地。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "48-s0317",
      "originalId": "s0317",
      "blockIndex": 48,
      "chinese": "金為疾，為內兵，亡地。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "48-s0318",
      "originalId": "s0318",
      "blockIndex": 48,
      "chinese": "三星若合，其宿地國外內有兵與喪，改立公王。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "48-s0319",
      "originalId": "s0319",
      "blockIndex": 48,
      "chinese": "四星合，兵喪并起，君子憂，小人流。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "48-s0320",
      "originalId": "s0320",
      "blockIndex": 48,
      "chinese": "五星合，是為易行，有德，受慶，改立大人，掩有四方，子孫蕃昌；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "48-s0321",
      "originalId": "s0321",
      "blockIndex": 48,
      "chinese": "無德，受殃若亡。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "48-s0322",
      "originalId": "s0322",
      "blockIndex": 48,
      "chinese": "五星皆大，其事亦大；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "48-s0323",
      "originalId": "s0323",
      "blockIndex": 48,
      "chinese": "皆小，事亦小。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "49-s0324",
      "originalId": "s0324",
      "blockIndex": 49,
      "chinese": "蚤出者為贏，贏者為客。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "49-s0325",
      "originalId": "s0325",
      "blockIndex": 49,
      "chinese": "晚出者為縮，縮者為主人。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "49-s0326",
      "originalId": "s0326",
      "blockIndex": 49,
      "chinese": "必有天應見於杓星。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "49-s0327",
      "originalId": "s0327",
      "blockIndex": 49,
      "chinese": "同舍為合。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "49-s0328",
      "originalId": "s0328",
      "blockIndex": 49,
      "chinese": "相陵為斗，七寸以內必之矣。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "50-s0329",
      "originalId": "s0329",
      "blockIndex": 50,
      "chinese": "五星色白圜，為喪旱；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "50-s0330",
      "originalId": "s0330",
      "blockIndex": 50,
      "chinese": "赤圜，則中不平，為兵；",
      "literal": "",
      "idiomatic": ""
    }
  ]
}