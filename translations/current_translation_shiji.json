  "sentences": [
    {
      "id": "17-s0121",
      "originalId": "s0121",
      "blockIndex": 17,
      "chinese": "廁下一星，曰天矢。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0122",
      "originalId": "s0122",
      "blockIndex": 17,
      "chinese": "矢黃則吉；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0123",
      "originalId": "s0123",
      "blockIndex": 17,
      "chinese": "青、白、黑，凶。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0124",
      "originalId": "s0124",
      "blockIndex": 17,
      "chinese": "其西有句曲九星，三處羅：一曰天旗，二曰天苑，三曰九游。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0125",
      "originalId": "s0125",
      "blockIndex": 17,
      "chinese": "其東有大星曰狼。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0126",
      "originalId": "s0126",
      "blockIndex": 17,
      "chinese": "狼角變色，多盜賊。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0127",
      "originalId": "s0127",
      "blockIndex": 17,
      "chinese": "下有四星曰弧，直狼。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0128",
      "originalId": "s0128",
      "blockIndex": 17,
      "chinese": "狼比地有大星，曰南極老人。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0129",
      "originalId": "s0129",
      "blockIndex": 17,
      "chinese": "老人見，治安；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0130",
      "originalId": "s0130",
      "blockIndex": 17,
      "chinese": "不見，兵起。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "17-s0131",
      "originalId": "s0131",
      "blockIndex": 17,
      "chinese": "常以秋分時候之于南郊。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "18-s0132",
      "originalId": "s0132",
      "blockIndex": 18,
      "chinese": "附耳入畢中，兵起。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "19-s0133",
      "originalId": "s0133",
      "blockIndex": 19,
      "chinese": "北宮玄武，虛、危。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "19-s0134",
      "originalId": "s0134",
      "blockIndex": 19,
      "chinese": "危為蓋屋；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "19-s0135",
      "originalId": "s0135",
      "blockIndex": 19,
      "chinese": "虛為哭泣之事。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}