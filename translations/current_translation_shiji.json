  "sentences": [
    {
      "id": "42-s0271",
      "originalId": "s0271",
      "blockIndex": 42,
      "chinese": "法，出東行十六舍而止；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "42-s0272",
      "originalId": "s0272",
      "blockIndex": 42,
      "chinese": "逆行二舍；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "42-s0273",
      "originalId": "s0273",
      "blockIndex": 42,
      "chinese": "六旬，復東行，自所止數十舍，十月而入西方；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "42-s0274",
      "originalId": "s0274",
      "blockIndex": 42,
      "chinese": "伏行五月，出東方。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "42-s0275",
      "originalId": "s0275",
      "blockIndex": 42,
      "chinese": "其出西方曰「反明」，主命者惡之。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "42-s0276",
      "originalId": "s0276",
      "blockIndex": 42,
      "chinese": "東行急，一日行一度半。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0277",
      "originalId": "s0277",
      "blockIndex": 43,
      "chinese": "其行東、西、南、北疾也。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0278",
      "originalId": "s0278",
      "blockIndex": 43,
      "chinese": "兵各聚其下；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0279",
      "originalId": "s0279",
      "blockIndex": 43,
      "chinese": "用戰，順之勝，逆之敗。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0280",
      "originalId": "s0280",
      "blockIndex": 43,
      "chinese": "熒惑從太白，軍憂；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0281",
      "originalId": "s0281",
      "blockIndex": 43,
      "chinese": "離之，軍卻。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0282",
      "originalId": "s0282",
      "blockIndex": 43,
      "chinese": "出太白陰，有分軍；",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0283",
      "originalId": "s0283",
      "blockIndex": 43,
      "chinese": "行其陽，有偏將戰。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0284",
      "originalId": "s0284",
      "blockIndex": 43,
      "chinese": "當其行，太白逮之，破軍殺將。",
      "literal": "",
      "idiomatic": ""
    },
    {
      "id": "43-s0285",
      "originalId": "s0285",
      "blockIndex": 43,
      "chinese": "其入守犯太微、軒轅、營室，主命惡之。",
      "literal": "",
      "idiomatic": ""
    }
  ]
}