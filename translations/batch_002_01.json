{
  "s0001": {
    "chinese": "五年冬十月，漢王追項羽至陽夏南止軍，與齊王信、魏相國越期會擊楚，至固陵，不會。",
    "literal": "[Literal translation needed for 33 characters]",
    "idiomatic": "[Idiomatic translation needed for 33 characters]"
  },
  "s0002": {
    "chinese": "楚擊漢軍，大破 之。",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0003": {
    "chinese": "漢王復入壁，深塹而守。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0004": {
    "chinese": "謂張良曰：「諸侯不從，柰何？」",
    "literal": "[Literal translation needed for 10 characters]",
    "idiomatic": "[Idiomatic translation needed for 10 characters]"
  },
  "s0005": {
    "chinese": "良對曰：「楚兵且破，未有分地，其不至固宜。",
    "literal": "[Literal translation needed for 16 characters]",
    "idiomatic": "[Idiomatic translation needed for 16 characters]"
  },
  "s0006": {
    "chinese": "君王能與共天下，可立致也。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0007": {
    "chinese": "齊王信之立，非君王 意，信亦不自堅。",
    "literal": "[Literal translation needed for 14 characters]",
    "idiomatic": "[Idiomatic translation needed for 14 characters]"
  },
  "s0008": {
    "chinese": "彭越本定梁地，始君王以魏豹故，拜越為相國。",
    "literal": "[Literal translation needed for 18 characters]",
    "idiomatic": "[Idiomatic translation needed for 18 characters]"
  },
  "s0009": {
    "chinese": "今豹死，越亦望王，而君王不早定。",
    "literal": "[Literal translation needed for 13 characters]",
    "idiomatic": "[Idiomatic translation needed for 13 characters]"
  },
  "s0010": {
    "chinese": "今能取睢陽以北至穀城皆以王彭越，從陳以東傅海與齊王信， 信家在楚，其意欲復得故邑。",
    "literal": "[Literal translation needed for 36 characters]",
    "idiomatic": "[Idiomatic translation needed for 36 characters]"
  },
  "s0011": {
    "chinese": "能出捐此地以許兩人，使各自為戰，則楚易敗也。」",
    "literal": "[Literal translation needed for 19 characters]",
    "idiomatic": "[Idiomatic translation needed for 19 characters]"
  },
  "s0012": {
    "chinese": "於是漢王發使使韓信、彭越。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0013": {
    "chinese": "至，皆引兵來。",
    "literal": "[Literal translation needed for 5 characters]",
    "idiomatic": "[Idiomatic translation needed for 5 characters]"
  },
  "s0014": {
    "chinese": "十一月，劉賈入楚地，圍壽春。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0015": {
    "chinese": "漢亦遣人誘楚大司馬周殷。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0016": {
    "chinese": "殷畔楚，以舒屠六，舉九江兵迎黥布，並行屠城父，隨劉賈皆會。",
    "literal": "[Literal translation needed for 24 characters]",
    "idiomatic": "[Idiomatic translation needed for 24 characters]"
  },
  "s0017": {
    "chinese": "十二月，圍羽垓下。",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0018": {
    "chinese": "羽夜聞漢軍四面皆楚歌，知盡得楚地，羽與數百騎走，是以兵大敗。",
    "literal": "[Literal translation needed for 26 characters]",
    "idiomatic": "[Idiomatic translation needed for 26 characters]"
  },
  "s0019": {
    "chinese": "灌嬰追斬羽東 城。",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0020": {
    "chinese": "楚地悉定，獨魯不下。",
    "literal": "[Literal translation needed for 8 characters]",
    "idiomatic": "[Idiomatic translation needed for 8 characters]"
  },
  "s0021": {
    "chinese": "漢王引天下兵欲屠之，為其守節禮義之國，乃持羽頭示其父兄，魯乃降。",
    "literal": "[Literal translation needed for 28 characters]",
    "idiomatic": "[Idiomatic translation needed for 28 characters]"
  },
  "s0022": {
    "chinese": "初，懷王封羽為魯公，及死，魯又為之堅守，故以魯公葬羽於穀 城。",
    "literal": "[Literal translation needed for 25 characters]",
    "idiomatic": "[Idiomatic translation needed for 25 characters]"
  },
  "s0023": {
    "chinese": "漢王為發葬，哭臨而去。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0024": {
    "chinese": "封項伯等四人為列侯，賜姓劉氏。",
    "literal": "[Literal translation needed for 13 characters]",
    "idiomatic": "[Idiomatic translation needed for 13 characters]"
  },
  "s0025": {
    "chinese": "諸民略在楚者皆歸之。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0026": {
    "chinese": "漢王還至定陶，馳入齊王信壁，奪其軍。",
    "literal": "[Literal translation needed for 15 characters]",
    "idiomatic": "[Idiomatic translation needed for 15 characters]"
  },
  "s0027": {
    "chinese": "初項羽所立臨江王共敖前死，子尉嗣 立為王，不降。",
    "literal": "[Literal translation needed for 20 characters]",
    "idiomatic": "[Idiomatic translation needed for 20 characters]"
  },
  "s0028": {
    "chinese": "遣盧綰、劉賈擊虜尉。",
    "literal": "[Literal translation needed for 8 characters]",
    "idiomatic": "[Idiomatic translation needed for 8 characters]"
  },
  "s0029": {
    "chinese": "春正月，追尊兄伯號曰武哀侯。",
    "literal": "[Literal translation needed for 12 characters]",
    "idiomatic": "[Idiomatic translation needed for 12 characters]"
  },
  "s0030": {
    "chinese": "下令曰：「楚地已定，義帝亡後，欲存恤楚眾，以定其主。",
    "literal": "[Literal translation needed for 20 characters]",
    "idiomatic": "[Idiomatic translation needed for 20 characters]"
  },
  "s0031": {
    "chinese": "齊王信習楚風 俗，更立為楚王，王淮北，都下邳。",
    "literal": "[Literal translation needed for 18 characters]",
    "idiomatic": "[Idiomatic translation needed for 18 characters]"
  },
  "s0032": {
    "chinese": "魏相國建城侯彭越勤勞魏民，卑下士卒，常以少擊眾，數破楚軍，其以魏故地王之，號曰梁王，都定陶。」",
    "literal": "[Literal translation needed for 39 characters]",
    "idiomatic": "[Idiomatic translation needed for 39 characters]"
  },
  "s0033": {
    "chinese": "又曰：「兵不得休八 年，萬民與苦甚，今天下事畢，其赦天下殊死以下。」",
    "literal": "[Literal translation needed for 26 characters]",
    "idiomatic": "[Idiomatic translation needed for 26 characters]"
  },
  "s0034": {
    "chinese": "於是諸侯上疏曰：「楚王韓信、韓王信、淮南王英布、梁王彭越、故衡山王吳芮、趙王張敖、燕王臧荼昧 死再拜言，大王陛下：先時秦為亡道，天下誅之。",
    "literal": "[Literal translation needed for 56 characters]",
    "idiomatic": "[Idiomatic translation needed for 56 characters]"
  },
  "s0035": {
    "chinese": "大王先得秦王，定關中，於天下功最多。",
    "literal": "[Literal translation needed for 15 characters]",
    "idiomatic": "[Idiomatic translation needed for 15 characters]"
  },
  "s0036": {
    "chinese": "存亡定危，救敗繼絕，以安萬民，功盛德厚。",
    "literal": "[Literal translation needed for 16 characters]",
    "idiomatic": "[Idiomatic translation needed for 16 characters]"
  },
  "s0037": {
    "chinese": "又加惠於諸侯王有功者，使 得立社稷。",
    "literal": "[Literal translation needed for 15 characters]",
    "idiomatic": "[Idiomatic translation needed for 15 characters]"
  },
  "s0038": {
    "chinese": "地分已定，而位號比儗，亡上下之分，大王功德之著，於後世不宣。",
    "literal": "[Literal translation needed for 25 characters]",
    "idiomatic": "[Idiomatic translation needed for 25 characters]"
  },
  "s0039": {
    "chinese": "昧死再拜上皇帝尊號。」",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0040": {
    "chinese": "漢王曰：「寡人聞帝者賢者有也，虛言亡實之名，非所取 也。",
    "literal": "[Literal translation needed for 22 characters]",
    "idiomatic": "[Idiomatic translation needed for 22 characters]"
  },
  "s0041": {
    "chinese": "今諸侯王皆推高寡人，將何以處之哉？」",
    "literal": "[Literal translation needed for 15 characters]",
    "idiomatic": "[Idiomatic translation needed for 15 characters]"
  },
  "s0042": {
    "chinese": "諸侯王皆曰：「大王起於細微，滅亂秦，威動海內。",
    "literal": "[Literal translation needed for 18 characters]",
    "idiomatic": "[Idiomatic translation needed for 18 characters]"
  },
  "s0043": {
    "chinese": "又以辟陋之地，自漢中行威德，誅不義，立有功，平定海內，功臣 皆受地食邑，非私之也。",
    "literal": "[Literal translation needed for 33 characters]",
    "idiomatic": "[Idiomatic translation needed for 33 characters]"
  },
  "s0044": {
    "chinese": "大王德施四海，諸侯王不足以道之，居帝位甚實宜，願大王以幸天下。」",
    "literal": "[Literal translation needed for 27 characters]",
    "idiomatic": "[Idiomatic translation needed for 27 characters]"
  },
  "s0045": {
    "chinese": "漢王曰：「諸侯王幸以為便於天下之民，則可矣。」",
    "literal": "[Literal translation needed for 18 characters]",
    "idiomatic": "[Idiomatic translation needed for 18 characters]"
  },
  "s0046": {
    "chinese": "於是諸侯王及 太尉長安侯臣綰等三百人，與博士稷嗣君叔孫通謹擇良日二月甲午，上尊號。",
    "literal": "[Literal translation needed for 37 characters]",
    "idiomatic": "[Idiomatic translation needed for 37 characters]"
  },
  "s0047": {
    "chinese": "漢王即皇帝位于氾水之陽。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0048": {
    "chinese": "尊王后曰皇后，太子曰皇太子，追尊先媼曰昭靈夫人。",
    "literal": "[Literal translation needed for 21 characters]",
    "idiomatic": "[Idiomatic translation needed for 21 characters]"
  },
  "s0049": {
    "chinese": "詔曰：「故衡山王吳芮與子二人、兄子一人，從百粵之兵，以佐諸侯，誅暴秦，有大功，諸侯立以為王。",
    "literal": "[Literal translation needed for 37 characters]",
    "idiomatic": "[Idiomatic translation needed for 37 characters]"
  },
  "s0050": {
    "chinese": "項羽侵奪之地，謂之番君。",
    "literal": "[Literal translation needed for 10 characters]",
    "idiomatic": "[Idiomatic translation needed for 10 characters]"
  }
}