{
  "s337": {
    "chinese": "三年六月甲辰封，十年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s338": {
    "chinese": "神爵元年，共侯玄成嗣，九年，有罪，削一級為關內侯，永光二年二月丁酉復以丞相侯，六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s339": {
    "chinese": "建昭三年，頃侯寬嗣。",
    "literal": "",
    "idiomatic": ""
  },
  "s340": {
    "chinese": "元延元年，釐侯育嗣。",
    "literal": "",
    "idiomatic": ""
  },
  "s341": {
    "chinese": "蕭侯湛嗣，元始中戶千四百二十，王莽敗，絕。",
    "literal": "",
    "idiomatic": ""
  },
  "s342": {
    "chinese": "平恩戴侯許廣漢",
    "literal": "",
    "idiomatic": ""
  },
  "s343": {
    "chinese": "以皇太子外祖父昌成君侯，五千六百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s344": {
    "chinese": "地節三年四月戊申封，七年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s345": {
    "chinese": "初元元年，共侯嘉以廣漢弟子中常侍紹侯，二十二年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s346": {
    "chinese": "河平一年，嚴侯況嗣。",
    "literal": "",
    "idiomatic": ""
  },
  "s347": {
    "chinese": "鴻嘉二年，質侯旦嗣，二十九年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s348": {
    "chinese": "建國四年，侯敬嗣，王莽敗，絕。",
    "literal": "",
    "idiomatic": ""
  },
  "s349": {
    "chinese": "高平憲侯魏相",
    "literal": "",
    "idiomatic": ""
  },
  "s350": {
    "chinese": "以丞相侯，八百一十三戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s351": {
    "chinese": "地節三年六月壬戌封，八年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s352": {
    "chinese": "神爵三年，侯弘嗣，六年，甘露元年，坐酎宗廟騎至司馬門，不敬，削爵一級為關內侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s355": {
    "chinese": "柘",
    "literal": "",
    "idiomatic": ""
  },
  "s356": {
    "chinese": "平昌節侯王無故",
    "literal": "",
    "idiomatic": ""
  },
  "s357": {
    "chinese": "以帝舅關內侯侯，六百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s358": {
    "chinese": "四年二月甲寅封，九年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s359": {
    "chinese": "五鳳元年，考侯接嗣，十六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s360": {
    "chinese": "永光三年，釐侯臨嗣，二十一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s361": {
    "chinese": "鴻嘉元年，侯獲嗣，三十八年，建武五年，詔書復獲。",
    "literal": "",
    "idiomatic": ""
  },
  "s363": {
    "chinese": "樂昌共侯王武",
    "literal": "",
    "idiomatic": ""
  },
  "s364": {
    "chinese": "以帝舅關內侯侯，六百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s365": {
    "chinese": "二月甲寅封，十四年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s366": {
    "chinese": "甘露二年，戾侯商嗣，二十七年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s367": {
    "chinese": "河平四年，侯安嗣，二十七年，元始三年，為王莽所殺。",
    "literal": "",
    "idiomatic": ""
  },
  "s369": {
    "chinese": "汝南",
    "literal": "",
    "idiomatic": ""
  },
  "s370": {
    "chinese": "陽城繆侯劉德",
    "literal": "",
    "idiomatic": ""
  },
  "s371": {
    "chinese": "以宗正關內侯行謹重為宗室率，侯，子安民以戶五百贖弟更生罪，減一等，定戶六百四十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s372": {
    "chinese": "四年三月甲寅封，十年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s373": {
    "chinese": "五鳳二年，節侯安民嗣，八年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s374": {
    "chinese": "初元元年，釐侯慶忌嗣，二十一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s375": {
    "chinese": "居攝元年，侯颯嗣，王莽敗，絕。",
    "literal": "",
    "idiomatic": ""
  },
  "s376": {
    "chinese": "汝南",
    "literal": "",
    "idiomatic": ""
  },
  "s377": {
    "chinese": "樂陵安侯史高",
    "literal": "",
    "idiomatic": ""
  },
  "s378": {
    "chinese": "以悼皇考舅子侍中關內侯與發霍氏姦，侯，二千三百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s379": {
    "chinese": "八月乙丑封，二十四年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s380": {
    "chinese": "永光二年，嚴侯術嗣，十一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s381": {
    "chinese": "建始二年，康侯崇嗣，四年薨，亡後。元延二年六月癸巳，侯淑以崇弟紹封，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s382": {
    "chinese": "元始四年，侯岑以高曾孫紹封，王莽敗，絕。",
    "literal": "",
    "idiomatic": ""
  },
  "s386": {
    "chinese": "武陽頃侯丹",
    "literal": "",
    "idiomatic": ""
  },
  "s387": {
    "chinese": "鴻嘉元年四月庚辰以帝為太子時輔導有舊恩侯，千三百戶，七年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s388": {
    "chinese": "永始四年，煬侯邯嗣，十一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s389": {
    "chinese": "元壽二年，侯獲嗣，更始元年為兵所殺。",
    "literal": "",
    "idiomatic": ""
  },
  "s390": {
    "chinese": "郯",
    "literal": "",
    "idiomatic": ""
  },
  "s391": {
    "chinese": "邛成共侯王奉光",
    "literal": "",
    "idiomatic": ""
  },
  "s392": {
    "chinese": "以皇后父關內侯侯，二千七百五十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s393": {
    "chinese": "元康二年三月癸未封，十八年薨。",
    "literal": "",
    "idiomatic": ""
  }
}