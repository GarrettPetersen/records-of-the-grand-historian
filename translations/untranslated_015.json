  "s1439": "二十三",
  "s1440": "二",
  "s1441": "六",
  "s1442": "十六",
  "s1443": "326",
  "s1444": "四十三",
  "s1445": "十二。初臘。會龍門。",
  "s1446": "九",
  "s1447": "七",
  "s1448": "二十四",
  "s1449": "三",
  "s1450": "七",
  "s1451": "十七",
  "s1452": "325",
  "s1453": "四十四",
  "s1454": "十三。四月戊午，君為王。",
  "s1455": "十",
  "s1456": "八。魏敗我韓舉。",
  "s1457": "趙武靈王元年。魏敗我趙護。",
  "s1458": "四",
  "s1459": "八",
  "s1460": "十八",
  "s1461": "324",
  "s1462": "四十五",
  "s1463": "相張儀將兵取陝。初更元年",
  "s1464": "十一。衛嗣君元年。",
  "s1465": "九",
  "s1466": "二。城鄗。",
  "s1467": "五",
  "s1468": "九",
  "s1469": "十九",
  "s1470": "323",
  "s1471": "四十六",
  "s1472": "二。相張儀與齊、楚會齧桑。",
  "s1473": "十二",
  "s1474": "十。君為王。",
  "s1475": "三",
  "s1476": "六。敗魏襄陵。",
  "s1477": "十。君為王。",
  "s1478": "齊湣王地元年",
  "s1479": "322",
  "s1480": "四十七",
  "s1481": "三。張儀免相，相魏。",
  "s1482": "十三。秦取曲沃。平周女化為丈夫。",
  "s1483": "十一",
  "s1484": "四。與韓會區鼠。",
  "s1485": "七",
  "s1486": "十一",
  "s1487": "二",
  "s1488": "321",
  "s1489": "四十八",
  "s1490": "四",
  "s1491": "十四",
  "s1492": "十二",
