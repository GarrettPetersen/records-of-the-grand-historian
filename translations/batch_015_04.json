{
  "s0901": "秦敗我將屈饨。",
  "s0902": "九。",
  "s0903": "燕人共立公子平。",
  "s0904": "十二",
  "s0905": "311 四 十四。",
  "s0906": "蜀相殺蜀侯。",
  "s0907": "八。",
  "s0908": "圍衛。",
  "s0909": "韓襄王元年 十五 十八 燕昭王元年 十三",
  "s0910": "310 五 秦武王元年。",
  "s0911": "誅蜀相壯。",
  "s0912": "張儀、魏章皆出之魏。",
  "s0913": "九。",
  "s0914": "與秦會臨晉。",
  "s0915": "二 十六。",
  "s0916": "吳廣入女，生子何，立為惠王后。",
  "s0917": "十九 二 十四",
  "s0918": "309 六 二。",
  "s0919": "初置丞相，樗里子、甘茂為丞相。",
  "s0920": "十。",
  "s0921": "張儀死。",
  "s0922": "三 十七 二十 三 十五",
  "s0923": "308 七 三 十一。",
  "s0924": "與秦會應。",
  "s0925": "四。",
  "s0926": "與秦會臨晉。",
  "s0927": "秦擊我宜陽。",
  "s0928": "十八 二十一 四 十六",
  "s0929": "307 八 四。",
  "s0930": "拔宜陽城，斬首六萬。",
  "s0931": "涉河，城武遂。",
  "s0932": "十二。",
  "s0933": "太子往朝秦。",
  "s0934": "五。",
  "s0935": "秦拔我宜陽，斬首六萬。",
  "s0936": "十九。",
  "s0937": "初胡服。",
  "s0938": "二十二 五 十七",
  "s0939": "306 九 秦昭襄王元年 十三。",
  "s0940": "秦擊皮氏，未拔而解。",
  "s0941": "六。",
  "s0942": "秦復與我武遂。",
  "s0943": "二十 二十三 六 十八",
  "s0944": "305 十 二。",
  "s0945": "彗星見。",
  "s0946": "桑君為亂，誅。",
  "s0947": "十四。",
  "s0948": "秦武王后來歸。",
  "s0949": "七 二十一 二十四。",
  "s0950": "秦來迎婦。",
  "s0951": "七 十九",
  "s0952": "304 十一 三 十五 八 二十二 二十五。",
  "s0953": "與秦王會黃觉，秦復歸我上庸。",
  "s0954": "八 二十",
  "s0955": "303 十二 四。",
  "s0956": "彗星見。",
  "s0957": "十六。",
  "s0958": "秦拔我蒲阪、晉陽、封陵。",
  "s0959": "九。",
  "s0960": "秦取武遂。",
  "s0961": "二十三 二十六。",
  "s0962": "太子質秦。",
  "s0963": "九 二十一",
  "s0964": "302 十三 五。",
  "s0965": "魏王來朝。",
  "s0966": "十七。",
  "s0967": "與秦會臨晉，復歸我蒲阪。",
  "s0968": "十。",
  "s0969": "太子嬰與秦王會臨晉，因至咸陽而歸。",
  "s0970": "二十四 二十七 十 二十二",
  "s0971": "301 十四 六。",
  "s0972": "蜀反，司馬錯往誅蜀守煇，定蜀。",
  "s0973": "日蝕，晝晦。",
  "s0974": "伐楚。",
  "s0975": "十八。",
  "s0976": "與秦擊楚。",
  "s0977": "十一。",
  "s0978": "秦取我穰。",
  "s0979": "與秦擊楚。",
  "s0980": "二十五。",
  "s0981": "趙攻中山。",
  "s0982": "惠后卒。",
  "s0983": "二十八。",
  "s0984": "秦、韓、魏、齊敗我將軍唐眛於重丘。",
  "s0985": "十一 二十三。",
  "s0986": "與秦擊楚，使公子將，大有功。",
  "s0987": "300 十五 七。",
  "s0988": "樗里疾卒。",
  "s0989": "擊楚，斬首三萬。",
  "s0990": "魏冉為相。",
  "s0991": "十九 十二 二十六 二十九。",
  "s0992": "秦取我襄城，殺景缺。",
  "s0993": "十二 二十四。",
  "s0994": "秦使涇陽君來為質。",
  "s0995": "299 十六 八。",
  "s0996": "楚王來，因留之。",
  "s0997": "二十。",
  "s0998": "與齊王會于韓。",
  "s0999": "十三。",
  "s1000": "齊、魏王來。",
}
