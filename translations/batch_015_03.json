{
  "s0601": "魏圍我邯鄲。",
  "s0602": "十六 八 二十五",
  "s0603": "353 十六 九 十八。",
  "s0604": "邯鄲降。",
  "s0605": "齊敗我桂陵。",
  "s0606": "六。",
  "s0607": "伐東周，取陵觀、廩丘。",
  "s0608": "二十二。",
  "s0609": "魏拔邯鄲。",
  "s0610": "十七 九 二十六。",
  "s0611": "敗魏桂陵。",
  "s0612": "352 十七 十。",
  "s0613": "衛公孫鞅為大良造，伐安邑，降之。",
  "s0614": "十九。",
  "s0615": "諸侯圍我襄陵。",
  "s0616": "築長城，塞固陽。",
  "s0617": "七 二十三 十八。",
  "s0618": "魯康公元年 十 二十七",
  "s0619": "351 十八 十一。",
  "s0620": "城商塞。",
  "s0621": "衛鞅圍固陽，降之。",
  "s0622": "二十。",
  "s0623": "歸趙邯鄲。",
  "s0624": "八。",
  "s0625": "申不害相。",
  "s0626": "二十四。",
  "s0627": "魏歸邯鄲，與魏盟漳水上。",
  "s0628": "十九 十一 二十八",
  "s0629": "350 十九 十二。",
  "s0630": "初聚小邑為三十一縣，令。",
  "s0631": "為田開阡陌。",
  "s0632": "二十一。",
  "s0633": "與秦遇彤。",
  "s0634": "九 二十五 二十 十二 二十九",
  "s0635": "349 二十 十三。",
  "s0636": "初為縣，有秩史。",
  "s0637": "二十二 十。",
  "s0638": "韓姬弒其君悼公。",
  "s0639": "趙肅侯元年。",
  "s0640": "二十一 十三 三十",
  "s0641": "348 二十一 十四。",
  "s0642": "初為賦。",
  "s0643": "二十三 十一。",
  "s0644": "昭侯如秦。",
  "s0645": "二 二十二 十四 三十一",
  "s0646": "347 二十二 十五 二十四 十二 三。",
  "s0647": "公子范襲邯鄲，不勝，死。",
  "s0648": "二十三 十五 三十二",
  "s0649": "346 二十三 十六 二十五 十三 四 二十四 十六 三十三。",
  "s0650": "殺其大夫牟辛。",
  "s0651": "345 二十四 十七 二十六 十四 五 二十五 十七 三十四",
  "s0652": "344 二十五。",
  "s0653": "諸侯會。",
  "s0654": "十八 二十七。",
  "s0655": "丹封名會。",
  "s0656": "丹，魏大臣。",
  "s0657": "十五 六 二十六 十八 三十五。",
  "s0658": "田忌襲齊，不勝。",
  "s0659": "343 二十六。",
  "s0660": "致伯秦。",
  "s0661": "十九。",
  "s0662": "城武城。",
  "s0663": "從東方牡丘來歸。",
  "s0664": "天子致伯。",
  "s0665": "二十八 十六 七 二十七。",
  "s0666": "魯景公偃元年。",
  "s0667": "十九 三十六",
  "s0668": "342 二十七 二十。",
  "s0669": "諸侯畢賀。",
  "s0670": "會諸侯于澤。",
  "s0671": "朝天子。",
  "s0672": "二十九。",
  "s0673": "中山君為相。",
  "s0674": "十七 八 二十八 二十 齊宣王辟彊元年",
  "s0675": "341 二十八 二十一。",
  "s0676": "馬生人。",
  "s0677": "三十。",
  "s0678": "齊虜我太子申，殺將軍龐涓。",
  "s0679": "十八 九 二十九 二十一 二。",
  "s0680": "敗魏馬陵。",
  "s0681": "田忌、田嬰、田馊將，孫子為師。",
  "s0682": "340 二十九 二十二。",
  "s0683": "封大良造商鞅。",
  "s0684": "三十一。",
  "s0685": "秦商君伐我，虜我公子卬。",
  "s0686": "十九 十 三十 二十二 三。",
  "s0687": "與趙會，伐魏。",
  "s0688": "339 三十 二十三。",
  "s0689": "與晉戰岸門。",
  "s0690": "三十二。",
  "s0691": "公子赫為太子。",
  "s0692": "二十 十一 楚威王熊商元年 二十三 四",
  "s0693": "338 三十一 二十四。",
  "s0694": "（秦）大荔圍合陽。",
  "s0695": "孝公薨。",
  "s0696": "商君反，死彤地。",
  "s0697": "三十三。",
  "s0698": "衛鞅亡歸我，我恐，弗內。",
  "s0699": "二十一 十二 二 二十四 五",
  "s0700": "337 三十二 秦惠文王元年。",
}
