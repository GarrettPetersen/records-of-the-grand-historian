{
  "s0051": {
    "chinese": "攝提格」。",
    "literal": "",
    "idiomatic": ""
  },
  "s0052": {
    "chinese": "亢為宗廟，主疾。",
    "literal": "",
    "idiomatic": ""
  },
  "s0053": {
    "chinese": "其南北兩大星，曰南門。",
    "literal": "",
    "idiomatic": ""
  },
  "s0054": {
    "chinese": "氐為天根，主疫。",
    "literal": "",
    "idiomatic": ""
  },
  "s0055": {
    "chinese": "尾為九子，曰君臣；",
    "literal": "",
    "idiomatic": ""
  },
  "s0056": {
    "chinese": "斥絕，不和。",
    "literal": "",
    "idiomatic": ""
  },
  "s0057": {
    "chinese": "箕為敖客，后妃之府，曰口舌。",
    "literal": "",
    "idiomatic": ""
  },
  "s0058": {
    "chinese": "火犯守角，則有戟。",
    "literal": "",
    "idiomatic": ""
  },
  "s0059": {
    "chinese": "房、心，王者惡之。",
    "literal": "",
    "idiomatic": ""
  },
  "s0060": {
    "chinese": "南宮朱鳥，權、衡。",
    "literal": "",
    "idiomatic": ""
  },
  "s0061": {
    "chinese": "衡、太微，三光之廷。",
    "literal": "",
    "idiomatic": ""
  },
  "s0062": {
    "chinese": "筐衛十二星，藩臣：西，將；",
    "literal": "",
    "idiomatic": ""
  },
  "s0063": {
    "chinese": "東，相；",
    "literal": "",
    "idiomatic": ""
  },
  "s0064": {
    "chinese": "南四星，執法；",
    "literal": "",
    "idiomatic": ""
  },
  "s0065": {
    "chinese": "中，端門；",
    "literal": "",
    "idiomatic": ""
  },
  "s0066": {
    "chinese": "左右，掖門。",
    "literal": "",
    "idiomatic": ""
  },
  "s0067": {
    "chinese": "掖門內六星，諸侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s0068": {
    "chinese": "其內五星，五帝坐。",
    "literal": "",
    "idiomatic": ""
  },
  "s0069": {
    "chinese": "後聚十五星，曰哀烏郎位；",
    "literal": "",
    "idiomatic": ""
  },
  "s0070": {
    "chinese": "旁一大星，將位也。",
    "literal": "",
    "idiomatic": ""
  },
  "s0071": {
    "chinese": "月、五星順入，軌道，司其出，所守，天子所誅也。",
    "literal": "",
    "idiomatic": ""
  },
  "s0072": {
    "chinese": "其逆入，若不軌道，以所犯名之；",
    "literal": "",
    "idiomatic": ""
  },
  "s0073": {
    "chinese": "中坐，成形，皆群下不從謀也。",
    "literal": "",
    "idiomatic": ""
  },
  "s0074": {
    "chinese": "金、火尤甚。",
    "literal": "",
    "idiomatic": ""
  },
  "s0075": {
    "chinese": "廷藩西有隨星四，名曰少微，士大夫。",
    "literal": "",
    "idiomatic": ""
  },
  "s0076": {
    "chinese": "權，軒轅，黃龍體。",
    "literal": "",
    "idiomatic": ""
  },
  "s0077": {
    "chinese": "前大星，女主象；",
    "literal": "",
    "idiomatic": ""
  },
  "s0078": {
    "chinese": "旁小星，御者後宮屬。",
    "literal": "",
    "idiomatic": ""
  },
  "s0079": {
    "chinese": "月、五星守犯者，如衡占。",
    "literal": "",
    "idiomatic": ""
  },
  "s0080": {
    "chinese": "東井為水事。",
    "literal": "",
    "idiomatic": ""
  },
  "s0081": {
    "chinese": "火入之，一星居其左右，天子且以火為敗。",
    "literal": "",
    "idiomatic": ""
  },
  "s0082": {
    "chinese": "東井西曲星曰戉；",
    "literal": "",
    "idiomatic": ""
  },
  "s0083": {
    "chinese": "北，北河；",
    "literal": "",
    "idiomatic": ""
  },
  "s0084": {
    "chinese": "南，南河；",
    "literal": "",
    "idiomatic": ""
  },
  "s0085": {
    "chinese": "兩河、天闕間為關梁。",
    "literal": "",
    "idiomatic": ""
  },
  "s0086": {
    "chinese": "輿鬼，鬼祠事；",
    "literal": "",
    "idiomatic": ""
  },
  "s0087": {
    "chinese": "中白者為質。",
    "literal": "",
    "idiomatic": ""
  },
  "s0088": {
    "chinese": "火守南北河，兵起，穀不登。",
    "literal": "",
    "idiomatic": ""
  },
  "s0089": {
    "chinese": "故德成衡，觀成潢，傷成戉，禍成井，誅成質。",
    "literal": "",
    "idiomatic": ""
  },
  "s0090": {
    "chinese": "柳為鳥喙，主木草。",
    "literal": "",
    "idiomatic": ""
  },
  "s0091": {
    "chinese": "七星，頸，為員宮，主急事。",
    "literal": "",
    "idiomatic": ""
  },
  "s0092": {
    "chinese": "張，嗉，為廚，主觴客。",
    "literal": "",
    "idiomatic": ""
  },
  "s0093": {
    "chinese": "翼為羽翮，主遠客。",
    "literal": "",
    "idiomatic": ""
  },
  "s0094": {
    "chinese": "軫為車，主風。",
    "literal": "",
    "idiomatic": ""
  },
  "s0095": {
    "chinese": "其旁有一小星，曰長沙，星星不欲明；",
    "literal": "",
    "idiomatic": ""
  },
  "s0096": {
    "chinese": "明與四星等，若五星入軫中，兵大起。",
    "literal": "",
    "idiomatic": ""
  },
  "s0097": {
    "chinese": "軫南眾星曰天庫，庫有五車。",
    "literal": "",
    "idiomatic": ""
  },
  "s0098": {
    "chinese": "車星角，若益眾，及不具，亡處車馬。",
    "literal": "",
    "idiomatic": ""
  },
  "s0099": {
    "chinese": "西宮咸池，曰天五潢。",
    "literal": "",
    "idiomatic": ""
  },
  "s0100": {
    "chinese": "五潢，五帝車舍。",
    "literal": "",
    "idiomatic": ""
  }
}