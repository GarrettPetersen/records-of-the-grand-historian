  "s637": {
    "chinese": "四十四",
    "literal": "",
    "idiomatic": ""
  },
  "s638": {
    "chinese": "高后五年，侯簡嗣，二十九年，孝景三年，謀反，誅。戶三千。",
    "literal": "",
    "idiomatic": ""
  },
  "s639": {
    "chinese": "孫",
    "literal": "",
    "idiomatic": ""
  },
  "s640": {
    "chinese": "曾孫",
    "literal": "",
    "idiomatic": ""
  },
  "s641": {
    "chinese": "元康四年，止玄孫長陵不更廣世詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s642": {
    "chinese": "祁穀侯繒賀",
    "literal": "",
    "idiomatic": ""
  },
  "s643": {
    "chinese": "以執盾漢王三年初起從晉陽，以連敖擊項籍。漢王敗走，賀擊楚迫騎，以故不得進。漢王顧謂賀祈王。戰彭城，斬項籍，爭惡，絕延璧，侯，千四戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s644": {
    "chinese": "六月丁亥封，三十三年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s645": {
    "chinese": "五十一",
    "literal": "",
    "idiomatic": ""
  },
  "s646": {
    "chinese": "孝文十二年，頃侯胡嗣，十七年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s647": {
    "chinese": "孝景六年，侯它嗣，十九年，元光二年，坐射擅罷，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s648": {
    "chinese": "曾孫",
    "literal": "",
    "idiomatic": ""
  },
  "s649": {
    "chinese": "元康四年，賀玄孫茂陵公大夫賜詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s650": {
    "chinese": "平悼侯工師喜",
    "literal": "",
    "idiomatic": ""
  },
  "s651": {
    "chinese": "初以舍人從擊破秦，以郎中入漢，以將軍定諸侯，守雒陽，侯，比費侯賀，千三百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s652": {
    "chinese": "六月丁亥封，六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s653": {
    "chinese": "三十二位次曰聊城侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s654": {
    "chinese": "十二年，靖侯奴嗣，三十一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s655": {
    "chinese": "孝文十六年，侯執嗣，十九年，孝景中五年，坐匿死罪，會赦，免。戶三千三百。",
    "literal": "",
    "idiomatic": ""
  },
  "s658": {
    "chinese": "魯侯奚涓",
    "literal": "",
    "idiomatic": ""
  },
  "s659": {
    "chinese": "以舍人從起沛，至咸陽為郎，入漢，以將軍定諸侯，四千八百戶，功比舞陽侯，死軍事。",
    "literal": "",
    "idiomatic": ""
  },
  "s660": {
    "chinese": "重平六年，侯涓亡子，封母底為侯，十九年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s661": {
    "chinese": "七",
    "literal": "",
    "idiomatic": ""
  },
  "s666": {
    "chinese": "城父嚴侯尹恢",
    "literal": "",
    "idiomatic": ""
  },
  "s667": {
    "chinese": "初以謁者從入漢，以將軍擊定諸侯，以右丞相備守淮陽，功比厭次侯，二千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s668": {
    "chinese": "六年封，九年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s669": {
    "chinese": "二十六",
    "literal": "",
    "idiomatic": ""
  },
  "s670": {
    "chinese": "孝惠三年，侯開方嗣，七年，高后三年，奪爵為關內侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s671": {
    "chinese": "孫",
    "literal": "",
    "idiomatic": ""
  },
  "s672": {
    "chinese": "曾孫",
    "literal": "",
    "idiomatic": ""
  },
  "s673": {
    "chinese": "玄孫",
    "literal": "",
    "idiomatic": ""
  },
  "s678": {
    "chinese": "六世元康四年，恢玄孫之子新豐簪褭殷詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s682": {
    "chinese": "任侯張越",
    "literal": "",
    "idiomatic": ""
  },
  "s683": {
    "chinese": "以騎都尉漢五年從起東垣，擊燕、代，屬雍齒，有功，為車騎將軍。",
    "literal": "",
    "idiomatic": ""
  },
  "s684": {
    "chinese": "六年封，十六年，高后三年，坐匿死罪，免。戶七百五十。",
    "literal": "",
    "idiomatic": ""
  },
  "s690": {
    "chinese": "棘丘侯襄",
    "literal": "",
    "idiomatic": ""
  },
  "s691": {
    "chinese": "以執盾隊史前元年從起碭，破秦，治粟內史入漢，以上郡守擊定西魏地，功侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s692": {
    "chinese": "六年封，十四年，高后元年，有罪，免。戶九百七十。",
    "literal": "",
    "idiomatic": ""
  },
  "s698": {
    "chinese": "河陵頃侯郭亭",
    "literal": "",
    "idiomatic": ""
  },
  "s699": {
    "chinese": "以連敖前元年從起單父，以塞路入漢，還定三秦，屬周呂侯，以都尉擊項籍，功侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s700": {
    "chinese": "七月庚寅封，二十四年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s701": {
    "chinese": "二十七",
    "literal": "",
    "idiomatic": ""
  },
  "s702": {
    "chinese": "孝文三年，惠侯歐嗣，二十二年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s703": {
    "chinese": "孝景二年，勝侯客嗣，八年，有罪，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s711": {
    "chinese": "南中六年，靖侯延居紹封，十五年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s712": {
    "chinese": "元光六年，侯則嗣，十七年，元鼎五年，坐酎金免。",
    "literal": "",
    "idiomatic": ""
  },
  "s713": {
    "chinese": "元康四年，亭玄孫茂陵公乘賢詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s714": {
    "chinese": "昌武靖信侯單究",
    "literal": "",
    "idiomatic": ""
  },
  "s715": {
    "chinese": "初以舍人從，以郎入漢，定三秦，以郎騎將軍擊諸侯，侯，九百戶，功比魏其侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s716": {
    "chinese": "七月庚寅封，十三年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s717": {
    "chinese": "四十五",
    "literal": "",
    "idiomatic": ""
  },
  "s718": {
    "chinese": "孝惠六年，惠侯如意嗣，四十三年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s719": {
    "chinese": "孝景中元四年，侯賈成嗣，十六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s720": {
    "chinese": "元光五年，侯德嗣，四年，元朔三年，坐傷人二旬內死，棄市。戶六百。",
    "literal": "",
    "idiomatic": ""
  },
  "s726": {
    "chinese": "六世",
    "literal": "",
    "idiomatic": ""
  },
  "s727": {
    "chinese": "七世元康四年，究玄孫之孫陽陵公乘萬年詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s730": {
    "chinese": "高宛制侯丙猜",
    "literal": "",
    "idiomatic": ""
  },
  "s731": {
    "chinese": "初以客從入漢，定三秦，以中尉破項籍，侯，千六百五戶，比斥丘侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s732": {
    "chinese": "七月戊戌封，七年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s733": {
    "chinese": "四十一",
    "literal": "",
    "idiomatic": ""
  },
  "s734": {
    "chinese": "孝惠元年，簡侯得嗣，三十年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s735": {
    "chinese": "孝文十六年，平侯武嗣，二十四年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s736": {
    "chinese": "建元元年，侯信嗣，三年，坐出入屬車間，免。戶三千二百。",
    "literal": "",
    "idiomatic": ""
  },
  "s742": {
    "chinese": "六世",
    "literal": "",
    "idiomatic": ""
  },
  "s743": {
    "chinese": "七世元康四年，猜玄孫之孫高宛大夫齮詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s744": {
    "chinese": "八世元始三年，猜玄孫之曾孫內詔賜爵關內侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s746": {
    "chinese": "宣曲齊侯丁義",
    "literal": "",
    "idiomatic": ""
  },
  "s747": {
    "chinese": "以卒從起留，以騎將入漢，定三秦，破籍軍滎陽，為郎騎將，破鍾離眛軍固陵，侯，六百七十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s748": {
    "chinese": "七月戊戌封，三十二年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s749": {
    "chinese": "四十三",
    "literal": "",
    "idiomatic": ""
  },
  "s750": {
    "chinese": "發婁孝文十一年，侯通嗣，十七年，有罪，赦為鬼薪。戶千一百。孝景中五年，通復封，十一年，有罪，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s751": {
    "chinese": "孫",
    "literal": "",
    "idiomatic": ""
  },
  "s752": {
    "chinese": "元康四年，義曾孫陽安公士年詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s754": {
    "chinese": "終陵齊侯華毋害",
    "literal": "",
    "idiomatic": ""
  },
  "s755": {
    "chinese": "以越將從起留，入漢，定三秦，擊臧荼，侯，七百四十戶。從攻馬邑及布。",
    "literal": "",
    "idiomatic": ""
  },
  "s756": {
    "chinese": "七月戊戌封，三十五年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s757": {
    "chinese": "四十六",
    "literal": "",
    "idiomatic": ""
  },
  "s758": {
    "chinese": "孝文四年，共侯勃嗣，十六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s759": {
    "chinese": "後四年，侯祿嗣，七年，孝景四年，坐出界，耐為司寇。戶千五百。",
    "literal": "",
    "idiomatic": ""
  },
  "s760": {
    "chinese": "元康四年，曾孫於陵大夫告詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s762": {
    "chinese": "東茅敬侯劉到",
    "literal": "",
    "idiomatic": ""
  },
  "s763": {
    "chinese": "以舍人從起碭，至霸上，以二隊入漢，定三秦，以都尉擊項籍，破臧荼，侯，捕韓王信，為將軍。益邑千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s764": {
    "chinese": "八年丙辰封，二十四年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s765": {
    "chinese": "四十八",
    "literal": "",
    "idiomatic": ""
  },
  "s766": {
    "chinese": "孝文三年，侯告嗣，十二年，十六年，坐事國人過員，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s767": {
    "chinese": "孫",
    "literal": "",
    "idiomatic": ""
  },
  "s768": {
    "chinese": "元康四年，到曾孫鮦陽公乘咸詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s770": {
    "chinese": "斥丘懿侯唐厲",
    "literal": "",
    "idiomatic": ""
  },
  "s771": {
    "chinese": "以舍人初從起豐，以左司馬入漢，以亞將攻籍，卻敵，為東部都尉，破籍，侯成武，為漢中尉，擊布，為斥丘侯，千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s772": {
    "chinese": "八月丙辰封，二十年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s773": {
    "chinese": "四十",
    "literal": "",
    "idiomatic": ""
  },
  "s774": {
    "chinese": "孝文九年，共侯朝嗣，十三年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s775": {
    "chinese": "後六年，侯賢嗣，四十三年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s776": {
    "chinese": "元鼎二年，侯尊嗣，二年，坐酎金免。",
    "literal": "",
    "idiomatic": ""
  },
  "s784": {
    "chinese": "元康四年，厲曾孫長安公士廣意詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s786": {
    "chinese": "臺定侯戴野",
    "literal": "",
    "idiomatic": ""
  },
  "s787": {
    "chinese": "以舍人從起碭，用隊率入漢，以都尉擊籍，籍死，擊臨江，屬將軍賈，功侯。以將軍擊燕、代。",
    "literal": "",
    "idiomatic": ""
  },
  "s788": {
    "chinese": "八月甲子封，二十五年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s789": {
    "chinese": "三十五",
    "literal": "",
    "idiomatic": ""
  },
  "s790": {
    "chinese": "孝文四年，侯午嗣，二十二年，孝景三年，坐謀反，誅。",
    "literal": "",
    "idiomatic": ""
  },
  "s791": {
    "chinese": "孫",
    "literal": "",
    "idiomatic": ""
  },
  "s792": {
    "chinese": "曾孫",
    "literal": "",
    "idiomatic": ""
  },
  "s793": {
    "chinese": "元康四年，野玄孫長陵上造安昌詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s794": {
    "chinese": "安國武侯王陵",
    "literal": "",
    "idiomatic": ""
  },
  "s795": {
    "chinese": "以自聚黨定南陽，漢王還擊項籍，以兵屬，從定天下，侯，五千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s796": {
    "chinese": "八月甲子封，二十一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s797": {
    "chinese": "十二",
    "literal": "",
    "idiomatic": ""
  },
  "s798": {
    "chinese": "高后八年，哀侯忌嗣，一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s799": {
    "chinese": "孝文元年，終侯斿嗣，三十九年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s800": {
    "chinese": "建元元年，安侯辟方嗣，二十年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s801": {
    "chinese": "元狩三年，侯定嗣，八年，元鼎五年，坐酎金免。",
    "literal": "",
    "idiomatic": ""
  },
  "s809": {
    "chinese": "元康四年，陵玄孫長安公乘襄詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s810": {
    "chinese": "樂成節侯丁禮",
    "literal": "",
    "idiomatic": ""
  },
  "s811": {
    "chinese": "以中涓騎從起碭，為騎將入漢，定三秦，為正奉侯，以都尉擊籍，屬灌嬰，殺龍且，更為樂成侯，千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s812": {
    "chinese": "八月甲子封，二十六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s813": {
    "chinese": "四十二",
    "literal": "",
    "idiomatic": ""
  },
  "s814": {
    "chinese": "孝文五年，夷侯馬從嗣，十八年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s815": {
    "chinese": "後七年，式侯吾客嗣，四十二年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s816": {
    "chinese": "元鼎二年，侯義嗣，三年，坐言五利侯不道，棄市。戶二千四百。",
    "literal": "",
    "idiomatic": ""
  },
  "s817": {
    "chinese": "玄孫",
    "literal": "",
    "idiomatic": ""
  },
  "s822": {
    "chinese": "六世",
    "literal": "",
    "idiomatic": ""
  },
  "s823": {
    "chinese": "七世元康四年，禮玄孫之孫長安公士禹詔復家。",
    "literal": "",
    "idiomatic": ""
  },
  "s826": {
    "chinese": "辟陽幽侯審食其",
    "literal": "",
    "idiomatic": ""
  },
  "s827": {
    "chinese": "以舍人初起，侍呂后、孝惠。二歲十月，呂后入楚，食其侍從一歲，侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s828": {
    "chinese": "八月甲子封，二十五年，為淮南王長所殺。",
    "literal": "",
