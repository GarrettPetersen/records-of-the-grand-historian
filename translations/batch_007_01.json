{
  "s0001": {
    "chinese": "孝武皇帝，景帝中子也，母曰王美人。",
    "literal": "[Literal translation needed for 14 characters]",
    "idiomatic": "[Idiomatic translation needed for 14 characters]"
  },
  "s0002": {
    "chinese": "年四歲立為膠東王。",
    "literal": "[Literal translation needed for 8 characters]",
    "idiomatic": "[Idiomatic translation needed for 8 characters]"
  },
  "s0003": {
    "chinese": "七歲為皇太子，母為皇后。",
    "literal": "[Literal translation needed for 10 characters]",
    "idiomatic": "[Idiomatic translation needed for 10 characters]"
  },
  "s0004": {
    "chinese": "十六歲，後三年正月，景帝崩。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0005": {
    "chinese": "甲子，太子即皇帝位，尊皇太后竇氏曰太皇太后，皇后曰皇太后。",
    "literal": "[Literal translation needed for 25 characters]",
    "idiomatic": "[Idiomatic translation needed for 25 characters]"
  },
  "s0006": {
    "chinese": "三月，封皇太后同母弟田蚡、勝皆為列侯。",
    "literal": "[Literal translation needed for 16 characters]",
    "idiomatic": "[Idiomatic translation needed for 16 characters]"
  },
  "s0007": {
    "chinese": "春二月，赦天下，賜民爵一級。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0008": {
    "chinese": "年八十復二算，九十復甲卒。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0009": {
    "chinese": "行三銖錢。",
    "literal": "[Literal translation needed for 4 characters]",
    "idiomatic": "[Idiomatic translation needed for 4 characters]"
  },
  "s0010": {
    "chinese": "夏四月己巳，詔曰：「古之立教，鄉里以齒，朝廷以爵，扶世導民，莫善於德。",
    "literal": "[Literal translation needed for 27 characters]",
    "idiomatic": "[Idiomatic translation needed for 27 characters]"
  },
  "s0011": {
    "chinese": "然則於鄉里先耆艾，奉高年，古之道也。",
    "literal": "[Literal translation needed for 15 characters]",
    "idiomatic": "[Idiomatic translation needed for 15 characters]"
  },
  "s0012": {
    "chinese": "今天下孝子順孫願自竭盡以承其親，外迫公事，內乏資財，是以孝心闕焉。",
    "literal": "[Literal translation needed for 29 characters]",
    "idiomatic": "[Idiomatic translation needed for 29 characters]"
  },
  "s0013": {
    "chinese": "朕甚哀之。",
    "literal": "[Literal translation needed for 4 characters]",
    "idiomatic": "[Idiomatic translation needed for 4 characters]"
  },
  "s0014": {
    "chinese": "民年九十以上，已有受鬻法，為復子若孫，令得身帥妻妾遂其供養之事。」",
    "literal": "[Literal translation needed for 28 characters]",
    "idiomatic": "[Idiomatic translation needed for 28 characters]"
  },
  "s0015": {
    "chinese": "五月，詔曰：「河海潤千里，其令祠官修山川之祠，為歲事，曲加禮。」",
    "literal": "[Literal translation needed for 24 characters]",
    "idiomatic": "[Idiomatic translation needed for 24 characters]"
  },
  "s0016": {
    "chinese": "赦吳楚七國帑輸在官者。",
    "literal": "[Literal translation needed for 10 characters]",
    "idiomatic": "[Idiomatic translation needed for 10 characters]"
  },
  "s0017": {
    "chinese": "秋七月，詔曰：「衛士轉置送迎二萬人，其省萬人。",
    "literal": "[Literal translation needed for 18 characters]",
    "idiomatic": "[Idiomatic translation needed for 18 characters]"
  },
  "s0018": {
    "chinese": "罷苑馬，以賜貧民。」",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0019": {
    "chinese": "議立明堂。",
    "literal": "[Literal translation needed for 4 characters]",
    "idiomatic": "[Idiomatic translation needed for 4 characters]"
  },
  "s0020": {
    "chinese": "遣使者安車蒲輪，束帛加璧，徵魯申公。",
    "literal": "[Literal translation needed for 15 characters]",
    "idiomatic": "[Idiomatic translation needed for 15 characters]"
  },
  "s0021": {
    "chinese": "二年冬十月，御史大夫趙綰坐請毋奏事太皇太后，及郎中令王臧皆下獄，自殺。",
    "literal": "[Literal translation needed for 31 characters]",
    "idiomatic": "[Idiomatic translation needed for 31 characters]"
  },
  "s0022": {
    "chinese": "丞相嬰、太尉蚡免。",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0023": {
    "chinese": "春二月丙戌朔，日有蝕之。",
    "literal": "[Literal translation needed for 10 characters]",
    "idiomatic": "[Idiomatic translation needed for 10 characters]"
  },
  "s0024": {
    "chinese": "夏四月戊申，有如日夜出。",
    "literal": "[Literal translation needed for 10 characters]",
    "idiomatic": "[Idiomatic translation needed for 10 characters]"
  },
  "s0025": {
    "chinese": "初置茂陵邑。",
    "literal": "[Literal translation needed for 5 characters]",
    "idiomatic": "[Idiomatic translation needed for 5 characters]"
  },
  "s0026": {
    "chinese": "三年春，河水溢于平原，大飢，人相食。",
    "literal": "[Literal translation needed for 14 characters]",
    "idiomatic": "[Idiomatic translation needed for 14 characters]"
  },
  "s0027": {
    "chinese": "賜徙茂陵者戶錢二十萬，田二頃。",
    "literal": "[Literal translation needed for 13 characters]",
    "idiomatic": "[Idiomatic translation needed for 13 characters]"
  },
  "s0028": {
    "chinese": "初作便門橋。",
    "literal": "[Literal translation needed for 5 characters]",
    "idiomatic": "[Idiomatic translation needed for 5 characters]"
  },
  "s0029": {
    "chinese": "秋七月，有星孛于西北。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0030": {
    "chinese": "濟川王明坐殺太傅、中傅廢遷防陵。",
    "literal": "[Literal translation needed for 14 characters]",
    "idiomatic": "[Idiomatic translation needed for 14 characters]"
  },
  "s0031": {
    "chinese": "閩越圍東甌，東甌告急。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0032": {
    "chinese": "遣中大夫嚴助持節發會稽兵，浮海救之。",
    "literal": "[Literal translation needed for 16 characters]",
    "idiomatic": "[Idiomatic translation needed for 16 characters]"
  },
  "s0033": {
    "chinese": "未至，閩越走，兵還。",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0034": {
    "chinese": "九月丙子晦，日有蝕之。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0035": {
    "chinese": "四年夏，有風赤如血。",
    "literal": "[Literal translation needed for 8 characters]",
    "idiomatic": "[Idiomatic translation needed for 8 characters]"
  },
  "s0036": {
    "chinese": "六月，旱。",
    "literal": "[Literal translation needed for 3 characters]",
    "idiomatic": "[Idiomatic translation needed for 3 characters]"
  },
  "s0037": {
    "chinese": "秋九月，有星孛于東北。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0038": {
    "chinese": "五年春，罷三銖錢，行半兩錢。",
    "literal": "[Literal translation needed for 11 characters]",
    "idiomatic": "[Idiomatic translation needed for 11 characters]"
  },
  "s0039": {
    "chinese": "置五經博士。",
    "literal": "[Literal translation needed for 5 characters]",
    "idiomatic": "[Idiomatic translation needed for 5 characters]"
  },
  "s0040": {
    "chinese": "夏四月，平原君薨。",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0041": {
    "chinese": "五月，大蝗。",
    "literal": "[Literal translation needed for 4 characters]",
    "idiomatic": "[Idiomatic translation needed for 4 characters]"
  },
  "s0042": {
    "chinese": "秋八月，廣川王越、清河王乘皆薨。",
    "literal": "[Literal translation needed for 13 characters]",
    "idiomatic": "[Idiomatic translation needed for 13 characters]"
  },
  "s0043": {
    "chinese": "六年春二月乙未，遼東高廟災。",
    "literal": "[Literal translation needed for 12 characters]",
    "idiomatic": "[Idiomatic translation needed for 12 characters]"
  },
  "s0044": {
    "chinese": "夏四月壬子，高園便殿火。",
    "literal": "[Literal translation needed for 10 characters]",
    "idiomatic": "[Idiomatic translation needed for 10 characters]"
  },
  "s0045": {
    "chinese": "上素服五日。",
    "literal": "[Literal translation needed for 5 characters]",
    "idiomatic": "[Idiomatic translation needed for 5 characters]"
  },
  "s0046": {
    "chinese": "五月丁亥，太皇太后崩。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  },
  "s0047": {
    "chinese": "秋八月，有星孛于東方，長竟天。",
    "literal": "[Literal translation needed for 12 characters]",
    "idiomatic": "[Idiomatic translation needed for 12 characters]"
  },
  "s0048": {
    "chinese": "閩越王郢攻南越。",
    "literal": "[Literal translation needed for 7 characters]",
    "idiomatic": "[Idiomatic translation needed for 7 characters]"
  },
  "s0049": {
    "chinese": "遣大行王恢將兵出豫章，大司農韓安國出會稽，擊之。",
    "literal": "[Literal translation needed for 21 characters]",
    "idiomatic": "[Idiomatic translation needed for 21 characters]"
  },
  "s0050": {
    "chinese": "未至，越人殺郢降，兵還。",
    "literal": "[Literal translation needed for 9 characters]",
    "idiomatic": "[Idiomatic translation needed for 9 characters]"
  }
}