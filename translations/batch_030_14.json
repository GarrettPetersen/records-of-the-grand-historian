{
  "s0651": {
    "chinese": "訛言，誠然。",
    "literal": "",
    "idiomatic": ""
  },
  "s0652": {
    "chinese": "凡候歲美惡，謹候歲始。",
    "literal": "",
    "idiomatic": ""
  },
  "s0653": {
    "chinese": "歲始或冬至日，產氣始萌。",
    "literal": "",
    "idiomatic": ""
  },
  "s0654": {
    "chinese": "臘明日，人眾卒歲，壹會飲食，發陽氣，故曰初歲。",
    "literal": "",
    "idiomatic": ""
  },
  "s0655": {
    "chinese": "正月旦，王者歲首；",
    "literal": "",
    "idiomatic": ""
  },
  "s0656": {
    "chinese": "立春，四時之始也。",
    "literal": "",
    "idiomatic": ""
  },
  "s0657": {
    "chinese": "四始者，候之日。",
    "literal": "",
    "idiomatic": ""
  },
  "s0658": {
    "chinese": "而漢魏鮮集臘明正月旦決八風。",
    "literal": "",
    "idiomatic": ""
  },
  "s0659": {
    "chinese": "風從南，大旱；",
    "literal": "",
    "idiomatic": ""
  },
  "s0660": {
    "chinese": "西南，小旱；",
    "literal": "",
    "idiomatic": ""
  },
  "s0661": {
    "chinese": "西方，有兵；",
    "literal": "",
    "idiomatic": ""
  },
  "s0662": {
    "chinese": "西北，戎叔為，小雨，趣兵；",
    "literal": "",
    "idiomatic": ""
  },
  "s0663": {
    "chinese": "北方，為中歲；",
    "literal": "",
    "idiomatic": ""
  },
  "s0664": {
    "chinese": "東北，為上歲；",
    "literal": "",
    "idiomatic": ""
  },
  "s0665": {
    "chinese": "東方，大水；",
    "literal": "",
    "idiomatic": ""
  },
  "s0666": {
    "chinese": "東南，民有疾疫，歲惡。",
    "literal": "",
    "idiomatic": ""
  },
  "s0667": {
    "chinese": "故八風各與其衝對，課多者為勝。",
    "literal": "",
    "idiomatic": ""
  },
  "s0668": {
    "chinese": "多勝少，久勝亟，疾勝徐。",
    "literal": "",
    "idiomatic": ""
  },
  "s0669": {
    "chinese": "旦至食，為麥；",
    "literal": "",
    "idiomatic": ""
  },
  "s0670": {
    "chinese": "食至日跌，為疾；",
    "literal": "",
    "idiomatic": ""
  },
  "s0671": {
    "chinese": "跌至晡，為黍；",
    "literal": "",
    "idiomatic": ""
  },
  "s0672": {
    "chinese": "晡至下晡，為叔；",
    "literal": "",
    "idiomatic": ""
  },
  "s0673": {
    "chinese": "下晡至日入，為麻。",
    "literal": "",
    "idiomatic": ""
  },
  "s0674": {
    "chinese": "欲終日有雲，有風，有日，當其時，深而多實；",
    "literal": "",
    "idiomatic": ""
  },
  "s0675": {
    "chinese": "亡雲，有風日，當其時，淺而少實；",
    "literal": "",
    "idiomatic": ""
  },
  "s0676": {
    "chinese": "有雲風，亡日，當其時，深而少實；",
    "literal": "",
    "idiomatic": ""
  },
  "s0677": {
    "chinese": "有日，亡雲，不風，當其時者稼有敗。",
    "literal": "",
    "idiomatic": ""
  },
  "s0678": {
    "chinese": "如食頃，小敗；",
    "literal": "",
    "idiomatic": ""
  },
  "s0679": {
    "chinese": "孰五斗米頃，大敗。",
    "literal": "",
    "idiomatic": ""
  },
  "s0680": {
    "chinese": "風復起，有雲，其稼復起。",
    "literal": "",
    "idiomatic": ""
  },
  "s0681": {
    "chinese": "各以其時用雲色占種所宜。",
    "literal": "",
    "idiomatic": ""
  },
  "s0682": {
    "chinese": "雨雪，寒，歲惡。",
    "literal": "",
    "idiomatic": ""
  },
  "s0683": {
    "chinese": "是日光明，聽都邑人民之聲。",
    "literal": "",
    "idiomatic": ""
  },
  "s0684": {
    "chinese": "聲宮，則歲美，吉；",
    "literal": "",
    "idiomatic": ""
  },
  "s0685": {
    "chinese": "商，有兵；",
    "literal": "",
    "idiomatic": ""
  },
  "s0686": {
    "chinese": "徵，旱；",
    "literal": "",
    "idiomatic": ""
  },
  "s0687": {
    "chinese": "羽，水；",
    "literal": "",
    "idiomatic": ""
  },
  "s0688": {
    "chinese": "角，歲惡。",
    "literal": "",
    "idiomatic": ""
  },
  "s0689": {
    "chinese": "或從正月旦比數雨。",
    "literal": "",
    "idiomatic": ""
  },
  "s0690": {
    "chinese": "率日食一升，至七升而極；",
    "literal": "",
    "idiomatic": ""
  },
  "s0691": {
    "chinese": "過之，不占。",
    "literal": "",
    "idiomatic": ""
  },
  "s0692": {
    "chinese": "數至十二日，直其月，占水旱。",
    "literal": "",
    "idiomatic": ""
  },
  "s0693": {
    "chinese": "為其環域千里內占，即為天下候，竟正月。",
    "literal": "",
    "idiomatic": ""
  },
  "s0694": {
    "chinese": "月所離列宿，日、風、雲，占其國。",
    "literal": "",
    "idiomatic": ""
  },
  "s0695": {
    "chinese": "然必察太歲所在。",
    "literal": "",
    "idiomatic": ""
  },
  "s0696": {
    "chinese": "金，穰；",
    "literal": "",
    "idiomatic": ""
  },
  "s0697": {
    "chinese": "水，毀；",
    "literal": "",
    "idiomatic": ""
  },
  "s0698": {
    "chinese": "木，飢；",
    "literal": "",
    "idiomatic": ""
  },
  "s0699": {
    "chinese": "火，旱。",
    "literal": "",
    "idiomatic": ""
  },
  "s0700": {
    "chinese": "此其大經也。",
    "literal": "",
    "idiomatic": ""
  }
}