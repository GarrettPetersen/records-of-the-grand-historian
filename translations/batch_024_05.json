{
  "s0201": {
    "chinese": "竟全大功，撫安四極。",
    "literal": "",
    "idiomatic": ""
  },
  "s0202": {
    "chinese": "海內有姦，紛亂東北。",
    "literal": "",
    "idiomatic": ""
  },
  "s0203": {
    "chinese": "詔撫成師，武臣承德。",
    "literal": "",
    "idiomatic": ""
  },
  "s0204": {
    "chinese": "行樂交逆，簫、勺群慝。",
    "literal": "",
    "idiomatic": ""
  },
  "s0205": {
    "chinese": "肅為濟哉，蓋定燕國。",
    "literal": "",
    "idiomatic": ""
  },
  "s0206": {
    "chinese": "大海蕩蕩水所歸，高賢愉愉民所懷。",
    "literal": "",
    "idiomatic": ""
  },
  "s0207": {
    "chinese": "大山崔，百卉殖。",
    "literal": "",
    "idiomatic": ""
  },
  "s0208": {
    "chinese": "民何貴？",
    "literal": "",
    "idiomatic": ""
  },
  "s0209": {
    "chinese": "貴有德。",
    "literal": "",
    "idiomatic": ""
  },
  "s0210": {
    "chinese": "安其所，樂終產。",
    "literal": "",
    "idiomatic": ""
  },
  "s0211": {
    "chinese": "樂終產，世繼緒。",
    "literal": "",
    "idiomatic": ""
  },
  "s0212": {
    "chinese": "飛龍秋，游上天。",
    "literal": "",
    "idiomatic": ""
  },
  "s0213": {
    "chinese": "高賢愉，樂民人。",
    "literal": "",
    "idiomatic": ""
  },
  "s0214": {
    "chinese": "豐草葽，女羅施。",
    "literal": "",
    "idiomatic": ""
  },
  "s0215": {
    "chinese": "殒何如，誰能回！",
    "literal": "",
    "idiomatic": ""
  },
  "s0216": {
    "chinese": "大莫大，成教德；",
    "literal": "",
    "idiomatic": ""
  },
  "s0217": {
    "chinese": "長莫長，被無極。",
    "literal": "",
    "idiomatic": ""
  },
  "s0218": {
    "chinese": "雷震震，電燿燿。",
    "literal": "",
    "idiomatic": ""
  },
  "s0219": {
    "chinese": "明德鄉，治本約。",
    "literal": "",
    "idiomatic": ""
  },
  "s0220": {
    "chinese": "治本約，澤弘大。",
    "literal": "",
    "idiomatic": ""
  },
  "s0221": {
    "chinese": "加被寵，咸相保。",
    "literal": "",
    "idiomatic": ""
  },
  "s0222": {
    "chinese": "德施大，世曼壽。",
    "literal": "",
    "idiomatic": ""
  },
  "s0223": {
    "chinese": "都荔遂芳，窅窊桂華。",
    "literal": "",
    "idiomatic": ""
  },
  "s0224": {
    "chinese": "孝奏天儀，若日月光。",
    "literal": "",
    "idiomatic": ""
  },
  "s0225": {
    "chinese": "乘玄四龍，回馳北行。",
    "literal": "",
    "idiomatic": ""
  },
  "s0226": {
    "chinese": "羽旄殷盛，芬哉芒芒。",
    "literal": "",
    "idiomatic": ""
  },
  "s0227": {
    "chinese": "孝道隨世，我署文章。",
    "literal": "",
    "idiomatic": ""
  },
  "s0228": {
    "chinese": "桂華。",
    "literal": "",
    "idiomatic": ""
  },
  "s0229": {
    "chinese": "馮馮翼翼，承天之則。",
    "literal": "",
    "idiomatic": ""
  },
  "s0230": {
    "chinese": "吾易久遠，燭明四極。",
    "literal": "",
    "idiomatic": ""
  },
  "s0231": {
    "chinese": "慈惠所愛，美若休德。",
    "literal": "",
    "idiomatic": ""
  },
  "s0232": {
    "chinese": "杳杳冥冥，克綽永福。",
    "literal": "",
    "idiomatic": ""
  },
  "s0233": {
    "chinese": "美芳。",
    "literal": "",
    "idiomatic": ""
  },
  "s0234": {
    "chinese": "磑磑即即，師象山則。",
    "literal": "",
    "idiomatic": ""
  },
  "s0235": {
    "chinese": "烏呼孝哉，案撫戎國。",
    "literal": "",
    "idiomatic": ""
  },
  "s0236": {
    "chinese": "蠻夷竭歡，象來致福。",
    "literal": "",
    "idiomatic": ""
  },
  "s0237": {
    "chinese": "兼臨是愛，終無兵革。",
    "literal": "",
    "idiomatic": ""
  },
  "s0238": {
    "chinese": "嘉薦芳矣，告靈饗矣。",
    "literal": "",
    "idiomatic": ""
  },
  "s0239": {
    "chinese": "告靈既饗，德音孔臧。",
    "literal": "",
    "idiomatic": ""
  },
  "s0240": {
    "chinese": "惟德之臧，建侯之常。",
    "literal": "",
    "idiomatic": ""
  },
  "s0241": {
    "chinese": "承保天休，令問不忘。",
    "literal": "",
    "idiomatic": ""
  },
  "s0242": {
    "chinese": "皇皇鴻明，蕩侯休德。",
    "literal": "",
    "idiomatic": ""
  },
  "s0243": {
    "chinese": "嘉承天和，伊樂厥福。",
    "literal": "",
    "idiomatic": ""
  },
  "s0244": {
    "chinese": "在樂不荒，惟民之則。",
    "literal": "",
    "idiomatic": ""
  },
  "s0245": {
    "chinese": "浚則師德，下民咸殖。",
    "literal": "",
    "idiomatic": ""
  },
  "s0246": {
    "chinese": "令問在舊，孔容翼翼。",
    "literal": "",
    "idiomatic": ""
  },
  "s0247": {
    "chinese": "孔容之常，承帝之明。",
    "literal": "",
    "idiomatic": ""
  },
  "s0248": {
    "chinese": "下民之樂，子孫保光。",
    "literal": "",
    "idiomatic": ""
  },
  "s0249": {
    "chinese": "承順溫良，受帝之光。",
    "literal": "",
    "idiomatic": ""
  },
  "s0250": {
    "chinese": "嘉薦令芳，壽考不忘。",
    "literal": "",
    "idiomatic": ""
  }
}