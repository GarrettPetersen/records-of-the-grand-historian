{
  "s416": {
    "chinese": "五月壬子封。",
    "literal": "",
    "idiomatic": ""
  },
  "s417": {
    "chinese": "侯督嗣。",
    "literal": "",
    "idiomatic": ""
  },
  "s418": {
    "chinese": "釐侯崇嗣，陽朔二年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s420": {
    "chinese": "北屈",
    "literal": "",
    "idiomatic": ""
  },
  "s428": {
    "chinese": "梁期侯任破胡",
    "literal": "",
    "idiomatic": ""
  },
  "s429": {
    "chinese": "以屬國都尉間出擊匈奴將軍絫絺縵等侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s430": {
    "chinese": "五月辛巳封。",
    "literal": "",
    "idiomatic": ""
  },
  "s431": {
    "chinese": "侯當千嗣，太始四年，坐賣馬一匹賈錢十五萬，過平，臧五百以上，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s435": {
    "chinese": "膫侯畢取",
    "literal": "",
    "idiomatic": ""
  },
  "s436": {
    "chinese": "以南越將軍降侯，五百一十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s437": {
    "chinese": "六年三月乙酉封。",
    "literal": "",
    "idiomatic": ""
  },
  "s438": {
    "chinese": "侯奉義嗣，後二年，坐祝詛上，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s441": {
    "chinese": "南陽",
    "literal": "",
    "idiomatic": ""
  },
  "s442": {
    "chinese": "將梁侯楊僕",
    "literal": "",
    "idiomatic": ""
  },
  "s443": {
    "chinese": "以樓船將軍擊南越椎鋒卻敵侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s444": {
    "chinese": "三月乙酉封，四年，元封四年，坐為將軍擊朝鮮畏懦，入竹二萬箇，贖完為城旦。",
    "literal": "",
    "idiomatic": ""
  },
  "s449": {
    "chinese": "安道侯揭陽定",
    "literal": "",
    "idiomatic": ""
  },
  "s450": {
    "chinese": "以南越揭陽令聞漢兵至自定降，侯，六百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s451": {
    "chinese": "三月乙酉封。",
    "literal": "",
    "idiomatic": ""
  },
  "s452": {
    "chinese": "侯當時嗣，延和四年，坐殺人，棄市。",
    "literal": "",
    "idiomatic": ""
  },
  "s455": {
    "chinese": "南陽",
    "literal": "",
    "idiomatic": ""
  },
  "s456": {
    "chinese": "隨桃頃侯趙光",
    "literal": "",
    "idiomatic": ""
  },
  "s457": {
    "chinese": "以南越蒼梧王聞漢兵至，降，侯，三千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s458": {
    "chinese": "四月癸亥封，薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s459": {
    "chinese": "侯昌樂嗣，本始元年薨。嗣子有罪，不得代。",
    "literal": "",
    "idiomatic": ""
  },
  "s462": {
    "chinese": "元始五年，放以光玄孫紹封，千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s463": {
    "chinese": "湘成侯監居翁",
    "literal": "",
    "idiomatic": ""
  },
  "s464": {
    "chinese": "以南越桂林監聞漢兵破番禺，諭甌駱民四十餘萬降，侯，八百三十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s465": {
    "chinese": "五月壬申封。",
    "literal": "",
    "idiomatic": ""
  },
  "s466": {
    "chinese": "侯益昌嗣，五鳳四年，坐為九真太守盜使人出買犀、奴婢，臧百萬以上，不道，誅。",
    "literal": "",
    "idiomatic": ""
  },
  "s469": {
    "chinese": "堵陽",
    "literal": "",
    "idiomatic": ""
  },
  "s470": {
    "chinese": "海常嚴侯蘇弘",
    "literal": "",
    "idiomatic": ""
  },
  "s471": {
    "chinese": "以伏波司馬得南越王建德侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s472": {
    "chinese": "七月乙酉封，七年，太初元年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s477": {
    "chinese": "外石侯吳陽",
    "literal": "",
    "idiomatic": ""
  },
  "s478": {
    "chinese": "以故東越衍侯佐繇王功侯，千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s479": {
    "chinese": "元封元年正月壬午封，九年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s480": {
    "chinese": "太初四年，侯首嗣，十四年，後二年，坐祝詛上，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s483": {
    "chinese": "濟陽",
    "literal": "",
    "idiomatic": ""
  },
  "s484": {
    "chinese": "下鄜侯左將黃同",
    "literal": "",
    "idiomatic": ""
  },
  "s485": {
    "chinese": "以故甌駱左將斬西于王功侯，七百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s486": {
    "chinese": "四月丁酉封。",
    "literal": "",
    "idiomatic": ""
  },
  "s487": {
    "chinese": "侯奉漢嗣，後二年，坐祝詛上，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s490": {
    "chinese": "南陽",
    "literal": "",
    "idiomatic": ""
  },
  "s491": {
    "chinese": "繚嫈侯劉福",
    "literal": "",
    "idiomatic": ""
  },
  "s492": {
    "chinese": "以校尉從橫海將軍擊南越侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s493": {
    "chinese": "正月乙卯封，二年，有罪，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s498": {
    "chinese": "蓹兒嚴侯轅終古",
    "literal": "",
    "idiomatic": ""
  },
  "s499": {
    "chinese": "以軍卒斬東越徇北將軍侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s500": {
    "chinese": "閏月癸卯封，六年，太初元年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  }
}