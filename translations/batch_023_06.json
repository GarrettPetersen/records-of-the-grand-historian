{
  "s0501": {
    "chinese": "周天五十六萬二千一百二十。",
    "literal": "",
    "idiomatic": ""
  },
  "s0502": {
    "chinese": "以章月乘月法，得周天。",
    "literal": "",
    "idiomatic": ""
  },
  "s0503": {
    "chinese": "歲中十二。",
    "literal": "",
    "idiomatic": ""
  },
  "s0504": {
    "chinese": "以三統乘四時，得歲中。",
    "literal": "",
    "idiomatic": ""
  },
  "s0505": {
    "chinese": "月周二百五十四。",
    "literal": "",
    "idiomatic": ""
  },
  "s0506": {
    "chinese": "以章月加閏法，得月周。",
    "literal": "",
    "idiomatic": ""
  },
  "s0507": {
    "chinese": "朔望之會百三十五。",
    "literal": "",
    "idiomatic": ""
  },
  "s0508": {
    "chinese": "參天數二十五，兩地數三十，得朔望之會。",
    "literal": "",
    "idiomatic": ""
  },
  "s0509": {
    "chinese": "會月六千三百四十五。",
