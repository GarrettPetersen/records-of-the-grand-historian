  "s175": {
    "chinese": "舞陽",
    "literal": "",
    "idiomatic": ""
  },
  "s176": {
    "chinese": "若陽侯猛",
    "literal": "",
    "idiomatic": ""
  },
  "s177": {
    "chinese": "以匈奴相降侯，五百三十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s178": {
    "chinese": "十月癸巳封，五年，坐謀反入匈奴，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s182": {
    "chinese": "平氏",
    "literal": "",
    "idiomatic": ""
  },
  "s183": {
    "chinese": "平陵侯蘇建",
    "literal": "",
    "idiomatic": ""
  },
  "s184": {
    "chinese": "以都尉從車騎將軍擊匈奴功侯，元朔五年，用游擊將軍從大將軍，益封，凡一千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s185": {
    "chinese": "三月丙辰封，六年，坐為前將軍與翕侯信俱敗，獨身脫來歸，當斬，贖罪，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s189": {
    "chinese": "武當",
    "literal": "",
    "idiomatic": ""
  },
  "s190": {
    "chinese": "岸頭侯張次公",
    "literal": "",
    "idiomatic": ""
  },
  "s191": {
    "chinese": "以都尉從車騎將軍擊匈奴侯，從大將軍，益封，凡二千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s192": {
    "chinese": "五月己巳封，五年，元狩元年，坐與淮南王女陵姦，受財物，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s196": {
    "chinese": "皮氏",
    "literal": "",
    "idiomatic": ""
  },
  "s197": {
    "chinese": "涉安侯於單",
    "literal": "",
    "idiomatic": ""
  },
  "s198": {
    "chinese": "以匈奴單于太子降侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s199": {
    "chinese": "三年四月丙子封，五月薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s204": {
    "chinese": "昌武侯趙安稽",
    "literal": "",
    "idiomatic": ""
  },
  "s205": {
    "chinese": "以匈奴王降侯，以昌武侯從驃騎將軍擊左王，益封。",
    "literal": "",
    "idiomatic": ""
  },
  "s206": {
    "chinese": "四年七月庚申封，二十一年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s207": {
    "chinese": "太初元年，侯充國嗣，四年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s210": {
    "chinese": "舞陽",
    "literal": "",
    "idiomatic": ""
  },
  "s211": {
    "chinese": "襄城侯桀龍",
    "literal": "",
    "idiomatic": ""
  },
  "s212": {
    "chinese": "以匈奴相國降侯，四百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s213": {
    "chinese": "七月庚申封，三十二年，與浞野侯俱戰死事。",
    "literal": "",
    "idiomatic": ""
  },
  "s214": {
    "chinese": "太初三年，侯病已嗣，十五年，後二年，坐祝詛上，下獄瘐死。",
    "literal": "",
    "idiomatic": ""
  },
  "s217": {
    "chinese": "襄垣",
    "literal": "",
    "idiomatic": ""
  },
  "s218": {
    "chinese": "安樂侯李蔡",
    "literal": "",
    "idiomatic": ""
  },
  "s219": {
    "chinese": "以將軍再擊匈奴得王，侯，二千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s220": {
    "chinese": "四月乙巳封，六年，元狩五年，坐以丞相侵賣園陵道壖地，自殺。",
    "literal": "",
    "idiomatic": ""
  },
  "s224": {
    "chinese": "昌",
    "literal": "",
    "idiomatic": ""
  },
  "s225": {
    "chinese": "合騎侯公孫敖",
    "literal": "",
    "idiomatic": ""
  },
  "s226": {
    "chinese": "以護軍都尉三從大將軍擊匈奴，至右王庭得王侯，元朔六年，從大將軍，益封，九千五百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s227": {
    "chinese": "以五年四月丁未封，至元狩二年坐將兵擊匈奴與票騎將軍期後，畏懦當斬，贖罪。",
    "literal": "",
    "idiomatic": ""
  },
  "s231": {
    "chinese": "高城",
    "literal": "",
    "idiomatic": ""
  },
  "s232": {
    "chinese": "軹侯李朔",
    "literal": "",
    "idiomatic": ""
  },
  "s233": {
    "chinese": "以校尉三從大將軍擊匈奴，至右王庭得虜閼氏功侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s234": {
    "chinese": "四月乙卯封，六年，有罪，當免。",
    "literal": "",
    "idiomatic": ""
  },
  "s238": {
    "chinese": "西安",
    "literal": "",
    "idiomatic": ""
  },
  "s239": {
    "chinese": "從平侯公孫戎奴",
    "literal": "",
    "idiomatic": ""
  },
  "s240": {
    "chinese": "以校尉三從大將軍擊匈奴，至右王庭為鴈行上石山先登，侯，一千一百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s241": {
    "chinese": "四月乙卯封，三年，元狩二年，坐為上黨太守發兵擊匈奴不以聞，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s245": {
    "chinese": "樂昌",
    "literal": "",
    "idiomatic": ""
  },
  "s246": {
    "chinese": "隨城侯趙不虞",
    "literal": "",
    "idiomatic": ""
  },
  "s247": {
    "chinese": "以校尉三從大將軍擊匈奴，攻辰吾先登石亹，侯，七百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s248": {
    "chinese": "四月乙卯封，三年，元狩二年，坐為定襄都尉，匈奴敗，太守以聞非實，謾，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s252": {
    "chinese": "千乘",
    "literal": "",
    "idiomatic": ""
  },
  "s253": {
    "chinese": "博望侯張騫",
    "literal": "",
    "idiomatic": ""
  },
  "s254": {
    "chinese": "以校尉數從大將軍擊匈奴，知道水，及前使絕國大夏，侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s255": {
    "chinese": "六年三月甲辰封，元狩二年，坐以將軍擊匈奴畏懦，當斬，贖罪，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s260": {
    "chinese": "眾利侯郝賢",
    "literal": "",
    "idiomatic": ""
  }
}