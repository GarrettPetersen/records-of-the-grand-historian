{
  "s150": {
    "chinese": "以將軍擊匈奴取朔方侯，後破右賢王，益封，又封三子。皇后弟。",
    "literal": "",
    "idiomatic": ""
  },
  "s151": {
    "chinese": "元朔二年二月丙辰封，二十三年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s152": {
    "chinese": "宜春侯伉，五年四月丁未以青功封，元鼎元年坐撟制不害免，太初元年嗣侯，五年闌入宮，完為城旦。",
    "literal": "",
    "idiomatic": ""
  },
  "s170": {
    "chinese": "平津獻侯公孫弘",
    "literal": "",
    "idiomatic": ""
  },
  "s171": {
    "chinese": "以丞相詔所褒侯，三百七十三戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s172": {
    "chinese": "元朔三年十一月乙丑封，六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s173": {
    "chinese": "元狩三年，侯度嗣，十三年，元封四年，坐為山陽太守詔徵鉅野令史成不遣，完為城旦。",
    "literal": "",
    "idiomatic": ""
  },
  "s176": {
    "chinese": "高城",
    "literal": "",
    "idiomatic": ""
  },
  "s177": {
    "chinese": "冠軍景桓侯霍去病",
    "literal": "",
    "idiomatic": ""
  },
  "s178": {
    "chinese": "以校尉擊匈奴侯，後以將軍破祈連迎昆邪王，益封。皇后姊子。",
    "literal": "",
    "idiomatic": ""
  },
  "s179": {
    "chinese": "六年四月壬申封，七年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s180": {
    "chinese": "南陽元鼎元年，哀侯嬗嗣，七年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s181": {
    "chinese": "樂平侯山，地節二年四月癸巳以從祖祖父大將軍光功封，三千戶，四年，坐謀反，誅。",
    "literal": "",
    "idiomatic": ""
  },
  "s183": {
    "chinese": "東郡",
    "literal": "",
    "idiomatic": ""
  },
  "s191": {
    "chinese": "周子南君姬嘉",
    "literal": "",
    "idiomatic": ""
  },
  "s192": {
    "chinese": "以周後詔所褒侯，三千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s193": {
    "chinese": "元鼎四年十一月丁卯封，六年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s194": {
    "chinese": "元封四年，君置嗣，二十四年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s195": {
    "chinese": "始元四年，君當嗣，十六年，地節三年，坐使奴殺家丞，棄市。",
    "literal": "",
    "idiomatic": ""
  },
  "s197": {
    "chinese": "長社",
    "literal": "",
    "idiomatic": ""
  },
  "s212": {
    "chinese": "樂通侯欒大",
    "literal": "",
    "idiomatic": ""
  },
  "s213": {
    "chinese": "以方術詔所褒侯，三千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s214": {
    "chinese": "四年四月乙巳封，五年，坐罔上，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s218": {
    "chinese": "高平",
    "literal": "",
    "idiomatic": ""
  },
  "s219": {
    "chinese": "牧丘恬侯石慶",
    "literal": "",
    "idiomatic": ""
  },
  "s220": {
    "chinese": "以丞相及父萬石積行侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s221": {
    "chinese": "五年九月丁丑封，十年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s222": {
    "chinese": "太初三年，侯德嗣，二年，天漢元年，坐為太常失法罔上，祠不如令，完為城旦。",
    "literal": "",
    "idiomatic": ""
  },
  "s225": {
    "chinese": "平原",
    "literal": "",
    "idiomatic": ""
  },
  "s226": {
    "chinese": "富民定侯車千秋",
    "literal": "",
    "idiomatic": ""
  },
  "s227": {
    "chinese": "以丞相侯，八百戶，以遺詔益封，凡千六百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s228": {
    "chinese": "征和四年六月丁巳封，十二年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s229": {
    "chinese": "元鳳四年，侯順嗣，六年，本始三年，坐為虎牙將軍擊匈奴詐增虜獲，自殺。",
    "literal": "",
    "idiomatic": ""
  },
  "s232": {
    "chinese": "蘄",
    "literal": "",
    "idiomatic": ""
  },
  "s0234": {
    "chinese": "右孝武九人。",
    "literal": "",
    "idiomatic": ""
  },
  "s0235": {
    "chinese": "三人隨父，凡十二人。",
    "literal": "",
    "idiomatic": ""
  },
  "s242": {
    "chinese": "博陸宣成侯霍光",
    "literal": "",
    "idiomatic": ""
  },
  "s243": {
    "chinese": "以奉車都尉捕反者莽何羅侯，二千三百五十戶，後以大將軍益封，萬七千二百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s244": {
    "chinese": "始元二年正月壬寅封，十七年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s245": {
    "chinese": "地節二年四月癸卯，侯禹嗣，四年，謀反，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s247": {
    "chinese": "元始二年四月乙酉，侯陽以光從父昆弟之曾孫龍勒士伍紹封，三千戶，王莽篡位，絕。",
    "literal": "",
    "idiomatic": ""
  },
  "s248": {
    "chinese": "北海河間東郡",
    "literal": "",
    "idiomatic": ""
  },
  "s249": {
    "chinese": "安陽侯上官桀",
    "literal": "",
    "idiomatic": ""
  },
  "s250": {
    "chinese": "以騎都尉捕反者莽何羅侯，二千三百戶。女孫為皇后。",
    "literal": "",
    "idiomatic": ""
  },
  "s251": {
    "chinese": "正月壬寅封，五年，元鳳元年，反，誅。",
    "literal": "",
    "idiomatic": ""
  },
  "s252": {
    "chinese": "桑樂侯安始元五年六月辛丑以皇后父車騎將軍封，千五百戶，二年，反，誅。",
    "literal": "",
    "idiomatic": ""
  },
  "s255": {
    "chinese": "蕩陰千乘",
    "literal": "",
    "idiomatic": ""
  },
  "s256": {
    "chinese": "宜春敬侯王訢",
    "literal": "",
    "idiomatic": ""
  },
  "s257": {
    "chinese": "以丞相侯，子譚與大將軍光定策，益封，坐法削戶五百，定六百八戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s258": {
    "chinese": "元鳳四年二月乙丑封，二年薨。",
    "literal": "",
    "idiomatic": ""
  }
}