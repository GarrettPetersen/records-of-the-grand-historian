{
  "s505": {
    "chinese": "開陵侯建成",
    "literal": "",
    "idiomatic": ""
  },
  "s506": {
    "chinese": "以故東粵建成侯與繇王斬餘善侯，二千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s507": {
    "chinese": "閏月癸卯封。",
    "literal": "",
    "idiomatic": ""
  },
  "s508": {
    "chinese": "侯祿嗣，征和三年，坐舍衛太子所私幸女子，又祝詛上，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s511": {
    "chinese": "臨淮",
    "literal": "",
    "idiomatic": ""
  },
  "s512": {
    "chinese": "臨蔡侯孫都",
    "literal": "",
    "idiomatic": ""
  },
  "s513": {
    "chinese": "以南粵郎，漢軍破番禺，為伏波得南粵相呂嘉，侯，千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s514": {
    "chinese": "閏月癸卯封。",
    "literal": "",
    "idiomatic": ""
  },
  "s515": {
    "chinese": "侯襄嗣，太初元年，坐擊番禺奪人虜掠，死。",
    "literal": "",
    "idiomatic": ""
  },
  "s518": {
    "chinese": "河內",
    "literal": "",
    "idiomatic": ""
  },
  "s519": {
    "chinese": "東城侯居股",
    "literal": "",
    "idiomatic": ""
  },
  "s520": {
    "chinese": "以故東粵繇王斬東粵王餘善侯，萬戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s521": {
    "chinese": "閏月癸卯封，二十年，延和三年，坐衛太子舉兵謀反，要斬。",
    "literal": "",
    "idiomatic": ""
  },
  "s525": {
    "chinese": "九江",
    "literal": "",
    "idiomatic": ""
  },
  "s526": {
    "chinese": "無錫侯多軍",
    "literal": "",
    "idiomatic": ""
  },
  "s527": {
    "chinese": "以東粵將軍，漢兵至，棄軍降，侯，千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s528": {
    "chinese": "元年封。",
    "literal": "",
    "idiomatic": ""
  },
  "s529": {
    "chinese": "侯卯嗣，延和四年，坐與歸義趙文王將兵追反虜，到弘農擅棄兵還，贖罪，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s532": {
    "chinese": "會稽",
    "literal": "",
    "idiomatic": ""
  },
  "s533": {
    "chinese": "涉都侯喜",
    "literal": "",
    "idiomatic": ""
  },
  "s534": {
    "chinese": "以父棄故南海太守，漢兵至，以越邑降，子侯，二千四十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s535": {
    "chinese": "元年封，八年，太初二年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s539": {
    "chinese": "南陽",
    "literal": "",
    "idiomatic": ""
  },
  "s540": {
    "chinese": "平州侯王唊",
    "literal": "",
    "idiomatic": ""
  },
  "s541": {
    "chinese": "以朝鮮將，漢兵至，降，侯，千四百八十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s542": {
    "chinese": "三年四月丁卯封，四年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s546": {
    "chinese": "梁父",
    "literal": "",
    "idiomatic": ""
  },
  "s547": {
    "chinese": "荻苴侯韓陶",
    "literal": "",
    "idiomatic": ""
  },
  "s548": {
    "chinese": "以朝鮮相將，漢兵圍之，降，侯五百四十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s549": {
    "chinese": "四月丁卯封，十九年，延和二年薨，封終身，不得嗣。",
    "literal": "",
    "idiomatic": ""
  },
  "s553": {
    "chinese": "勃海",
    "literal": "",
    "idiomatic": ""
  },
  "s554": {
    "chinese": "澅清侯參",
    "literal": "",
    "idiomatic": ""
  },
  "s555": {
    "chinese": "以朝鮮尼谿相使人殺其王右渠，降，侯，千戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s556": {
    "chinese": "六月丙辰封，十一年，天漢二年，坐匿朝鮮亡虜，下獄病死。",
    "literal": "",
    "idiomatic": ""
  },
  "s560": {
    "chinese": "齊",
    "literal": "",
    "idiomatic": ""
  },
  "s561": {
    "chinese": "騠茲侯稽谷姑",
    "literal": "",
    "idiomatic": ""
  },
  "s562": {
    "chinese": "以小月氏右苴王將眾降，侯，千九百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s563": {
    "chinese": "四年十一月丁未封，三年，太初元年薨，亡後。",
    "literal": "",
    "idiomatic": ""
  },
  "s567": {
    "chinese": "琅邪",
    "literal": "",
    "idiomatic": ""
  },
  "s568": {
    "chinese": "浩侯王恢",
    "literal": "",
    "idiomatic": ""
  },
  "s569": {
    "chinese": "以故中郎將將兵捕得車師王，侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s570": {
    "chinese": "正月甲申封，一月，坐使酒泉矯制害，當死，贖罪，免。",
    "literal": "",
    "idiomatic": ""
  },
  "s575": {
    "chinese": "瓡讘侯杆者",
    "literal": "",
    "idiomatic": ""
  },
  "s576": {
    "chinese": "以小月氏王將軍眾千騎降，侯，七百六十戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s577": {
    "chinese": "正月乙酉封，二年薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s578": {
    "chinese": "六月，侯勝嗣，五年，天漢二年薨，制所幸封，不得嗣。",
    "literal": "",
    "idiomatic": ""
  },
  "s581": {
    "chinese": "河東",
    "literal": "",
    "idiomatic": ""
  },
  "s582": {
    "chinese": "幾侯張阍",
    "literal": "",
    "idiomatic": ""
  },
  "s583": {
    "chinese": "以朝鮮王子漢兵圍朝鮮降侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s584": {
    "chinese": "三年癸未封，六年，使朝鮮，謀反，格死。",
    "literal": "",
    "idiomatic": ""
  }
}