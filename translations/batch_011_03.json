{
  "s0201": {
    "chinese": "常侍閎前為大司農中丞，數奏昌陵不可成。",
    "literal": "",
    "idiomatic": ""
  },
  "s0202": {
    "chinese": "侍中衛尉長數白宜早止，徙家反故處。",
    "literal": "",
    "idiomatic": ""
  },
  "s0203": {
    "chinese": "朕以長言下閎章，公卿議者皆合長計。",
    "literal": "",
    "idiomatic": ""
  },
  "s0204": {
    "chinese": "首建至策，閎典主省大費，民以康寧。",
    "literal": "",
    "idiomatic": ""
  },
  "s0205": {
    "chinese": "閎前賜爵關內侯，黃金百斤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0206": {
    "chinese": "其賜長爵關內侯，食邑千戶，閎五百戶。",
    "literal": "",
    "idiomatic": ""
  },
  "s0207": {
    "chinese": "萬年佞邪不忠，毒流眾庶，海內怨望，至今不息，雖蒙赦令，不宜居京師。",
    "literal": "",
    "idiomatic": ""
  },
  "s0208": {
    "chinese": "其徙萬年敦煌郡。」",
    "literal": "",
    "idiomatic": ""
  },
  "s0209": {
    "chinese": "是歲，御史大夫王駿卒。",
    "literal": "",
    "idiomatic": ""
  },
  "s0210": {
    "chinese": "三年春正月己卯晦，日有蝕之。",
    "literal": "",
    "idiomatic": ""
  },
  "s0211": {
    "chinese": "詔曰：「天災仍重，朕甚懼焉。",
    "literal": "",
    "idiomatic": ""
  },
  "s0212": {
    "chinese": "惟民之失職，臨遣大中大夫嘉等循行天下，存問耆老，民所疾苦。",
    "literal": "",
    "idiomatic": ""
  },
  "s0213": {
    "chinese": "其與部刺史舉惇樸遜讓有行義者各一人。」",
    "literal": "",
    "idiomatic": ""
  },
  "s0214": {
    "chinese": "冬十月庚辰，皇太后詔有司復甘泉泰畤、汾陰后土、雍五畤、陳倉陳寶祠。",
    "literal": "",
    "idiomatic": ""
  },
  "s0215": {
    "chinese": "語在郊祀志。",
    "literal": "",
    "idiomatic": ""
  },
  "s0216": {
    "chinese": "十一月，尉氏男子樊並等十三人謀反，殺陳留太守，劫略吏民，自稱將軍。",
    "literal": "",
    "idiomatic": ""
  },
  "s0217": {
    "chinese": "徒李譚等五人共格殺並等，皆封為列侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s0218": {
    "chinese": "十二月，山陽鐵官徒蘇令等二百二十八人攻殺長吏，盜庫兵，自稱將軍，經歷郡國十九，殺東郡太守、汝南都尉。",
    "literal": "",
    "idiomatic": ""
  },
  "s0219": {
    "chinese": "遣丞相長史、御史中丞持節督趣逐捕。",
    "literal": "",
    "idiomatic": ""
  },
  "s0220": {
    "chinese": "汝南太守嚴訢捕斬令等。",
    "literal": "",
    "idiomatic": ""
  },
  "s0221": {
    "chinese": "遷訢為大司農，賜黃金百斤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0222": {
    "chinese": "四年春正月，行幸甘泉，郊泰畤，神光降集紫殿。",
    "literal": "",
    "idiomatic": ""
  },
  "s0223": {
    "chinese": "大赦天下。",
    "literal": "",
    "idiomatic": ""
  },
  "s0224": {
    "chinese": "賜雲陽吏民爵，女子百戶牛酒，鰥寡孤獨高年帛。",
    "literal": "",
    "idiomatic": ""
  },
  "s0225": {
    "chinese": "三月，行幸河東，祠后土，賜吏民如雲陽，行所過無出田租。",
    "literal": "",
    "idiomatic": ""
  },
  "s0226": {
    "chinese": "夏四月癸未，長樂臨華殿、未央宮東司馬門皆災。",
    "literal": "",
    "idiomatic": ""
  },
  "s0227": {
    "chinese": "六月甲午，霸陵園門闕災。",
    "literal": "",
    "idiomatic": ""
  },
  "s0228": {
    "chinese": "出杜陵諸未嘗御者歸家。",
    "literal": "",
    "idiomatic": ""
  },
  "s0229": {
    "chinese": "詔曰：「乃者，地震京師，火災婁降，朕甚懼之。",
    "literal": "",
    "idiomatic": ""
  },
  "s0230": {
    "chinese": "有司其悉心明對厥咎，朕將親覽焉。」",
    "literal": "",
    "idiomatic": ""
  },
  "s0231": {
    "chinese": "又曰：「聖王明禮制以序尊卑，異車服以章有德，雖有其財，而無其尊，不得踰制，故民興行，上義而下利。",
    "literal": "",
    "idiomatic": ""
  },
  "s0232": {
    "chinese": "方今世俗奢僭罔極，靡有厭足。",
    "literal": "",
    "idiomatic": ""
  },
  "s0233": {
    "chinese": "公卿列侯親屬近臣，四方所則，未聞修身遵禮，同心憂國者也。",
    "literal": "",
    "idiomatic": ""
  },
  "s0234": {
    "chinese": "或乃奢侈逸豫，務廣第宅，治園池，多畜奴婢，被服綺穀，設鐘鼓，備女樂，車服嫁娶葬埋過制。",
    "literal": "",
    "idiomatic": ""
  },
  "s0235": {
    "chinese": "吏民慕效，娅以成俗，而欲望百姓儉節，家給人足，豈不難哉！",
    "literal": "",
    "idiomatic": ""
  },
  "s0236": {
    "chinese": "詩不云乎？",
    "literal": "",
    "idiomatic": ""
  },
  "s0237": {
    "chinese": "『赫赫師尹，民具爾瞻。』",
    "literal": "",
    "idiomatic": ""
  },
  "s0238": {
    "chinese": "其申敕有司，以漸禁之。",
    "literal": "",
    "idiomatic": ""
  },
  "s0239": {
    "chinese": "青綠民所常服，且勿止。",
    "literal": "",
    "idiomatic": ""
  },
  "s0240": {
    "chinese": "列侯近臣，各自省改。",
    "literal": "",
    "idiomatic": ""
  },
  "s0241": {
    "chinese": "司隸校尉察不變者。」",
    "literal": "",
    "idiomatic": ""
  },
  "s0242": {
    "chinese": "秋七月辛未晦，日有蝕之。",
    "literal": "",
    "idiomatic": ""
  },
  "s0243": {
    "chinese": "元延元年春正月己亥朔，日有蝕之。",
    "literal": "",
    "idiomatic": ""
  },
  "s0244": {
    "chinese": "三月，行幸雍，祠五畤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0245": {
    "chinese": "夏四月丁酉，無雲有雷，聲光耀耀，四面下至地，昏止。",
    "literal": "",
    "idiomatic": ""
  },
  "s0246": {
    "chinese": "赦天下。",
    "literal": "",
    "idiomatic": ""
  },
  "s0247": {
    "chinese": "秋七月，有星孛于東井。",
    "literal": "",
    "idiomatic": ""
  },
  "s0248": {
    "chinese": "詔曰：「乃者，日蝕星隕，謫見于天，大異重仍。",
    "literal": "",
    "idiomatic": ""
  },
  "s0249": {
    "chinese": "在位默然，罕有忠言。",
    "literal": "",
    "idiomatic": ""
  },
  "s0250": {
    "chinese": "今孛星見于東井，朕甚懼焉。",
    "literal": "",
    "idiomatic": ""
  },
  "s0251": {
    "chinese": "公卿大夫、博士、議郎其各悉心，惟思變意，明以經對，無有所諱；",
    "literal": "",
    "idiomatic": ""
  },
  "s0252": {
    "chinese": "與內郡國舉方正能直言極諫者各一人，北邊二十二郡舉勇猛知兵法者各一人。」",
    "literal": "",
    "idiomatic": ""
  },
  "s0253": {
    "chinese": "封蕭相國後喜為酇侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s0254": {
    "chinese": "冬十二月辛亥，大司馬大將軍王商薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s0255": {
    "chinese": "是歲，昭儀趙氏害後宮皇子。",
    "literal": "",
    "idiomatic": ""
  },
  "s0256": {
    "chinese": "二年春正月，行幸甘泉，郊泰畤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0257": {
    "chinese": "三月，行幸河東，祠后土。",
    "literal": "",
    "idiomatic": ""
  },
  "s0258": {
    "chinese": "夏四月，立廣陵孝王子守為王。",
    "literal": "",
    "idiomatic": ""
  },
  "s0259": {
    "chinese": "冬，行幸長楊宮，從胡客大校獵。",
    "literal": "",
    "idiomatic": ""
  },
  "s0260": {
    "chinese": "宿萯陽宮，賜從官。",
    "literal": "",
    "idiomatic": ""
  },
  "s0261": {
    "chinese": "三年春正月丙寅，蜀郡岷山崩，雍江三日，江水竭。",
    "literal": "",
    "idiomatic": ""
  },
  "s0262": {
    "chinese": "二月，封侍中衛尉淳于長為定陵侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s0263": {
    "chinese": "三月，行幸雍，祠五畤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0264": {
    "chinese": "四年春正月，行幸甘泉，郊泰畤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0265": {
    "chinese": "二月，罷司隸校尉官。",
    "literal": "",
    "idiomatic": ""
  },
  "s0266": {
    "chinese": "三月，行幸河東，祠后土。",
    "literal": "",
    "idiomatic": ""
  },
  "s0267": {
    "chinese": "甘露降京師，賜長安民牛酒。",
    "literal": "",
    "idiomatic": ""
  },
  "s0268": {
    "chinese": "綏和元年春正月，大赦天下。",
    "literal": "",
    "idiomatic": ""
  },
  "s0269": {
    "chinese": "二月癸丑，詔曰：「朕承太祖鴻業，奉宗廟二十五年，德不能綏理宇內，百姓怨望者眾。",
    "literal": "",
    "idiomatic": ""
  },
  "s0270": {
    "chinese": "不蒙天祐，至今未有繼嗣，天下無所係心。",
    "literal": "",
    "idiomatic": ""
  },
  "s0271": {
    "chinese": "觀于往古近事之戒，禍亂之萌，皆由斯焉。",
    "literal": "",
    "idiomatic": ""
  },
  "s0272": {
    "chinese": "定陶王欣於朕為子，慈仁孝順，可以承天序，繼祭祀。",
    "literal": "",
    "idiomatic": ""
  },
  "s0273": {
    "chinese": "其立欣為皇太子。",
    "literal": "",
    "idiomatic": ""
  },
  "s0274": {
    "chinese": "封中山王舅諫大夫馮參為宜鄉侯，益中山國三萬戶，以慰其意。",
    "literal": "",
    "idiomatic": ""
  },
  "s0275": {
    "chinese": "賜諸侯王、列侯金，天下當為父後者爵，三老、孝弟力田帛，各有差。」",
    "literal": "",
    "idiomatic": ""
  },
  "s0276": {
    "chinese": "又曰：「蓋聞王者必存二王之後，所以通三統也。",
    "literal": "",
    "idiomatic": ""
  },
  "s0277": {
    "chinese": "昔成湯受命，列為三代，而祭祀廢絕。",
    "literal": "",
    "idiomatic": ""
  },
  "s0278": {
    "chinese": "考求其後，莫正孔吉。",
    "literal": "",
    "idiomatic": ""
  },
  "s0279": {
    "chinese": "其封吉為殷紹嘉侯。」",
    "literal": "",
    "idiomatic": ""
  },
  "s0280": {
    "chinese": "三月，進爵為公，及周承休侯皆為公，地各百里。",
    "literal": "",
    "idiomatic": ""
  },
  "s0281": {
    "chinese": "行幸雍，祠五畤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0282": {
    "chinese": "夏四月，以大司馬票騎大將軍根為大司馬，罷將軍官。",
    "literal": "",
    "idiomatic": ""
  },
  "s0283": {
    "chinese": "御史大夫為大司空，封為列侯。",
    "literal": "",
    "idiomatic": ""
  },
  "s0284": {
    "chinese": "益大司馬、大司空奉如丞相。",
    "literal": "",
    "idiomatic": ""
  },
  "s0285": {
    "chinese": "秋八月庚戌，中山王興薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s0286": {
    "chinese": "冬十一月，立楚孝王孫景為定陶王。",
    "literal": "",
    "idiomatic": ""
  },
  "s0287": {
    "chinese": "定陵侯淳于長大逆不道，下獄死。",
    "literal": "",
    "idiomatic": ""
  },
  "s0288": {
    "chinese": "廷尉孔光使持節賜貴人許氏藥，飲藥死。",
    "literal": "",
    "idiomatic": ""
  },
  "s0289": {
    "chinese": "十二月，罷部刺史，更置州牧，秩二千石。",
    "literal": "",
    "idiomatic": ""
  },
  "s0290": {
    "chinese": "二年春正月，行幸甘泉，郊泰畤。",
    "literal": "",
    "idiomatic": ""
  },
  "s0291": {
    "chinese": "二月壬子，丞相翟方進薨。",
    "literal": "",
    "idiomatic": ""
  },
  "s0292": {
    "chinese": "三月，行幸河東，祠后土。",
    "literal": "",
    "idiomatic": ""
  },
  "s0293": {
    "chinese": "丙戌，帝崩于未央宮。",
    "literal": "",
    "idiomatic": ""
  },
  "s0294": {
    "chinese": "皇太后詔有司復長安南北郊。",
    "literal": "",
    "idiomatic": ""
  },
  "s0295": {
    "chinese": "四月己卯，葬延陵。",
    "literal": "",
    "idiomatic": ""
  },
  "s0299": {
    "chinese": "遭世承平，上下和睦。",
    "literal": "",
    "idiomatic": ""
  },
  "s0300": {
    "chinese": "然湛于酒色，趙氏亂內，外家擅朝，言之可為於邑。",
    "literal": "",
    "idiomatic": ""
  },
  "s0301": {
    "chinese": "建始以來，王氏始執國命，哀、平短祚，莽遂篡位，蓋其威福所由來者漸矣！",
    "literal": "",
    "idiomatic": ""
  }
}