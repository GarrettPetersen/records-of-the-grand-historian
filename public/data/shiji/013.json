        {
          "id": "s105",
          "content": "成王誦",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s106",
          "content": "魯周公旦，武王弟。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s107",
          "content": "齊太公尚，文王、武王師。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s108",
          "content": "晉唐叔虞，武王子。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s109",
          "content": "秦惡來，助紂。父飛廉，有力。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s110",
          "content": "楚熊繹。繹父鬻熊，事文王。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s111",
          "content": "宋微子啟，紂庶兄。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s112",
          "content": "衛康叔，武王弟。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s113",
          "content": "陳胡公滿，舜之後。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s114",
          "content": "蔡叔度，武王弟。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s115",
          "content": "曹叔振鐸，武王弟。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        },
        {
          "id": "s116",
          "content": "燕召公奭，周同姓。初封。",
          "translator": "Garrett M. Petersen (2026)",
          "model": "grok-code-fast-1"
        }
      ]
